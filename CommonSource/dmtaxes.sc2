	PROCEDURE list
		LPARAMETERS cworkarea
		
		IF USED('taxtmp')
		   USE IN taxtmp
		ENDIF
		IF USED('taxtmp1')
		   USE IN taxtmp1
		ENDIF
		
		LOCAL lcList
		PRIV llOK
		
		llOK = .T.
		
		SELECT ctable,cdescription FROM sevtax INTO CURSOR taxtmp1
		
		USE DBF('taxtmp1') AGAIN IN 0 ALIAS taxtmp EXCL
		SELECT taxtmp
		INDEX ON ctable TAG ctable
		INDEX ON cdescription TAG cdescrip
		
		IF _TALLY > 0
		
		   lcList = 'cTable\Code,cdescription/cdescrip\Table Description'
		
		   DO FORM ..\CUSTOM\picklist WITH 'taxtmp', lcList, THISFORM.txtState.VALUE, 2, .T., .T.
		
		   IF llOK
		      SELECT sevtax
		      SET ORDER TO ctable
		      SEEK(taxtmp.ctable)
		      THISFORM.REFRESH()
		   ENDIF
		ENDIF
		
	ENDPROC

