	PROCEDURE CMDATAMANAGER.beforedelete
		LPARAMETERS cworkarea
		
		SELECT wells
		LOCATE FOR cTable = sevtax.cTable
		IF FOUND()
		   THISFORM.oMessage.Warning('There are wells using this tax table.  It cannot be deleted.')
		   RETURN .F.
		ENDIF
		
		DODEFAULT(cworkarea)
		
	ENDPROC

	PROCEDURE CMDATAMANAGER.beforeupdate
		LPARAMETERS cworkarealist
		
		IF THISFORM.txtState.VALUE = '**'  &&  Don't let them use **, since it will screw up reporting later
		   THISFORM.oMessage.Warning('You cannot use ** as the table code, since that code is reserved for internal system use.')
		   RETURN .F.
		ENDIF
		
		DODEFAULT(cworkarealist)
		
	ENDPROC

	PROCEDURE txtState.Valid
		
		*
		*  Check to see if there are any deleted records or
		*  existing records with the same state code.
		*
		lcState = THIS.VALUE
		
		IF NOT EMPTY(lcState) AND lcState <> sevtax.ctable
		   SET DELETED OFF
		   SELECT sevtax1
		   SET ORDER TO cTable
		   IF SEEK(lcState)
		      IF NOT DELETED()
		         IF THISFORM.omessage.CONFIRM('There is already a tax table with this code.  Would you like to view the existing record?')
		            THISFORM.cmdatamanager.revertall()
		            SELECT sevtax
		            SET ORDER TO cTable
		            SEEK(lcState)
		            THISFORM.REFRESH()
		         ELSE
		            THIS.VALUE = ''
		            RETURN 0
		         ENDIF
		      ELSE
		         THISFORM.cmdatamanager.revertall()
		         SELECT sevtax
		         SET ORDER TO cTable
		         SEEK(lcState)
		         RECALL
		         SCATTER MEMVAR BLANK
		         GATHER MEMVAR
		         REPLACE cTable WITH lcState
		         THISFORM.REFRESH()
		      ENDIF
		   ENDIF
		   SET DELETED ON
		   SELECT sevtax
		ENDIF
		
		DODEFAULT()
		
	ENDPROC

