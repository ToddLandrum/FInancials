		   IF SEEK(lcWellID)   
		      thisform.txtWellName.value = cWellName
		      thisform.txtWellName.Refresh()
		      thisform.txtwellid.refresh()
		      thisform.pageframe.page2.pageFrameOwnInts.revenue.chkSpecify.Refresh()   
		   ENDIF   
		ELSE
