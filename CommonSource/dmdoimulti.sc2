		         CASE cRevClass = '7'
		            thisform.pageframe.page2.pageframeownints.revenue.lblRevInt7.CAPTION = allt(revcat.crevdesc)
		         CASE cRevClass = '8'
		            thisform.pageframe.page2.pageframeownints.revenue.lblRevOTH.CAPTION = allt(revcat.crevdesc)      
