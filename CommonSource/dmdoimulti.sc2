		* Set the labels to match the revenue category they are assigned
		   swselect('revcat')
		   SCAN FOR NOT EMPTY(cRevClass)
		      DO CASE
		         CASE cRevClass = '1'
		            thisform.pageframe.page2.pageframeownints.revenue.lblRevInt1.CAPTION = allt(revcat.crevdesc)
		         CASE cRevClass = '2'
		            thisform.pageframe.page2.pageframeownints.revenue.lblRevInt2.CAPTION = allt(revcat.crevdesc)
		         CASE cRevClass = '3'
		            thisform.pageframe.page2.pageframeownints.revenue.lblRevInt3.CAPTION = allt(revcat.crevdesc)
		         CASE cRevClass = '4'
		            thisform.pageframe.page2.pageframeownints.revenue.lblRevInt4.CAPTION = allt(revcat.crevdesc)
		         CASE cRevClass = '5'
		            thisform.pageframe.page2.pageframeownints.revenue.lblRevInt5.CAPTION = allt(revcat.crevdesc)
