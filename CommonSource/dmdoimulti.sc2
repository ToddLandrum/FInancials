	PROCEDURE cmdatamanager.afternav
		LPARAMETERS cworkarea
		
		DO CASE
			CASE THISFORM.PAGEFRAME.ACTIVEPAGE = 1   && Interest Views
				THISFORM.requeryclass('Wells')
				IF THISFORM.lIntWarning
					WAIT WIND NOWAIT 'This interest class is over 100%. Please correct it.'
				ENDIF
		
			CASE THISFORM.PAGEFRAME.ACTIVEPAGE = 2  && Owner Interests
				IF m.goapp.lPluggingModule
					THISFORM.PAGEFRAME.page2.pageFrameOwnInts.other.lblplugMod.VISIBLE  = .T.
					THISFORM.PAGEFRAME.page2.pageFrameOwnInts.other.txtnplugPct.VISIBLE = .T.
					THISFORM.PAGEFRAME.page2.pageFrameOwnInts.other.shpplug.VISIBLE = .T.
				ELSE
					THISFORM.PAGEFRAME.page2.pageFrameOwnInts.other.lblplugMod.VISIBLE  = .F.
					THISFORM.PAGEFRAME.page2.pageFrameOwnInts.other.txtnplugPct.VISIBLE = .F.
					THISFORM.PAGEFRAME.page2.pageFrameOwnInts.other.shpplug.VISIBLE = .F.
				ENDIF
				
				IF thisform.pageFrame.page2.chkLonhold.Value
				   thisform.pageFrame.page2.cbosuspreason.Enabled = .T.
				ELSE   
				   thisform.pageFrame.page2.cbosuspreason.Enabled = .F.
				ENDIF 
				
				thisform.ShowRevenueInterests()
		
				swselect('wells')
				LOCATE FOR cWellID = wellinv.cWellID
				IF FOUND()
				   thisform.txtwellname.Value = wells.cwellname
				ELSE
				   thisform.txtwellname.Value = 'Unknown'
				ENDIF 
				
				swselect('wellinv')
				THISFORM.PAGEFRAME.page2.txtcOwnerID.REFRESH()
		
		        swselect('doidecks')
		        LOCATE FOR cdeck = wellinv.cdeck AND cwellid = wellinv.cwellid
		        
				THISFORM.cboDECK.VALUE = wellinv.cdeck
				THISFORM.cboDECK.REFRESH()
		
				swselect('investor', .T.)
				SET ORDER TO cOwnerID
				IF SEEK(THISFORM.PAGEFRAME.page2.txtcOwnerID.VALUE)
					THISFORM.PAGEFRAME.page2.txtcOwnerName.VALUE = cownname
					THISFORM.PAGEFRAME.page2.txtcOwnerName.REFRESH()
					IF lDummy  &&  If a dummy owner, set the lDummyAfterNav flag, so we know it was set as a dummy when the record was moved onto - BH 01/31/08
						THISFORM.lDummyafternav = .T.
					ELSE
						THISFORM.lDummyafternav = .F.
					ENDIF
				ELSE
					THISFORM.PAGEFRAME.page2.txtcOwnerName.VALUE = 'Unknown'
					THISFORM.PAGEFRAME.page2.txtcOwnerName.REFRESH()
				ENDIF
				THISFORM.ShowFlatFields(wellinv.lflat)
				THISFORM.lcoldwellid   = wellinv.cWellID
				THISFORM.lcoldprogcode = wellinv.cProgCode
		
				IF UPPER(m.cworkarea) = 'WELLINV'
					THISFORM.oldinttype = wellinv.ljib
					THISFORM.setflat()
		
					IF wellinv.cTypeInv <> 'W'  &&  Not WI, so disable the working interest text box
						THISFORM.PAGEFRAME.page2.pageFrameOwnInts.expenses.txtInterest.SET('Enabled', .F.)
						THISFORM.PAGEFRAME.page2.pageFrameOwnInts.expenses.txtInterest.ENABLED = .F.
						THISFORM.PAGEFRAME.page2.chkLjib.SET('enabled', .F.)
						THISFORM.PAGEFRAME.page2.chkLjib.ENABLED = .F.
					ELSE
						THISFORM.PAGEFRAME.page2.pageFrameOwnInts.expenses.txtInterest.SET('Enabled', .T.)
						THISFORM.PAGEFRAME.page2.pageFrameOwnInts.expenses.txtInterest.ENABLED = .T.
						THISFORM.PAGEFRAME.page2.chkLjib.SET('enabled', .T.)
						THISFORM.PAGEFRAME.page2.chkLjib.ENABLED = .T.
					ENDIF
		
					IF wellinv.lpayout
						THISFORM.PAGEFRAME.page2.pageFrameOwnInts.expenses.txtapoint.BACKCOLOR = RGB(255, 0, 0)
					ELSE
						IF THISFORM.lexpliciteditmode = .T.
							THISFORM.PAGEFRAME.page2.pageFrameOwnInts.expenses.txtapoint.BACKCOLOR = RGB(212, 208, 200)
						ELSE
							THISFORM.PAGEFRAME.page2.pageFrameOwnInts.expenses.txtapoint.BACKCOLOR = RGB(255, 255, 255)
						ENDIF
					ENDIF
		
					THISFORM.cboDECK.REQUERY()
				ENDIF
				THISFORM.PAGEFRAME.page2.REFRESH()
			CASE THISFORM.PAGEFRAME.ACTIVEPAGE = 3  && Documents
				THISFORM.PAGEFRAME.page3.lstDocuments.REQUERY()
		
		ENDCASE
		
		THISFORM.LOCKSCREEN = .F.
		DODEFAULT(cworkarea)
		
		
	ENDPROC

