	PROCEDURE pgfOwnInfo.Page4.chkGLTrans.Click
		IF this.Value AND (INLIST(investor.ndisbfreq,2,3,4))
		   thisform.oMessage.Warning('This option cannot be marked for this owner, since their disbursement frequency is set to something other than monthly. When this option is used, suspense processing requires the owner to be marked as a monthly owner.')
		   this.Value = .f.
		ENDIF
		
		IF this.value = .t.
		   swselect('wellinv')
		   LOCATE FOR cownerid == investor.cOwnerID AND lOnHold
		   IF FOUND()
		      thisform.oMessage.warning('This option cannot be marked for this owner, since they have one or more interests that are currently on hold. When this option is used, suspense processing requires that the owner not be on hold.')
		      this.Value = .f.
		   ENDIF
		ENDIF 
		
		IF this.Value = .t. AND (m.goapp.lAMVersion OR m.goapp.lQBVersion)
		   IF this.Parent.chkLbackwith.Value = .t.
		      thisform.omessage.warning('This owner cannot be marked to post because they have backup ' + ;
		                                'withholding set. Remove the backup withholding and then mark this owner to post.')
		      this.set('value',.f.)
		      RETURN 
		   ENDIF
		ENDIF                           
		
		IF this.Value = .t. AND this.Parent.chkHold.Value
		   thisform.omessage.warning('This owner cannot be marked to post because they are on hold. ' + ;
		                         'Remove the hold check mark and then mark the posting option again.')
		   this.set('value',.f.)
		ENDIF 
		
		IF THIS.VALUE = .T.
		   THISFORM.oMessage.Warning('This option should only be marked for your company record, since it will create journal entries instead of creating a check when a run is closed.')
		   IF investor.lHold = .T. AND (m.goapp.lAMVersion OR m.goapp.lQBVersion)  &&  Don't let them put an owner who is set to post on hold - BH 10/02/2008
		      IF m.goapp.lQBVersion = .f.
		         THISFORM.oMessage.Warning('This owner is on hold.  They cannot be set to post to the General Ledger.')
		      ELSE
		         THISFORM.oMessage.Warning('This owner is on hold.  They cannot be set to post to the QB Income/Expense Accounts.')
		      ENDIF
		      this.set('value',.f.)
		   ENDIF
		
		ENDIF
		
		DODEFAULT()
		
	ENDPROC

