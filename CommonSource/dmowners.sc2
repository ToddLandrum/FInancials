	PROCEDURE pgfOwnInfo.Page3.cboFinCharge.Init
		*
		*  Builds the data displayed in the combo list box
		*
		LOCAL lcSelect, lnCount
		
		lcSelect = SELECT()
		
		SELECT nfinint, cfincode, cfincode FROM finchgs INTO ARRAY laFinchg ORDER BY cfincode
		lnCount = _tally
		
		THIS.ADDLISTITEM('NONE',1,1)
		THIS.ADDLISTITEM('',1,2)
		
		IF lnCount > 0
		   FOR lnX = 1 TO ALEN(laFinchg,1)
		      THIS.ADDLISTITEM(TRANSFORM(laFinChg[lnX,3])+'-'+STR(laFinChg[lnX,1],4,2) + ' Pct Per Year',lnX+1,1)
		      THIS.ADDLISTITEM(laFinChg[lnX,2],lnX+1,2)
		   ENDFOR
		ENDIF
		
		THIS.REFRESH()
		
		SELECT (lcSelect)
		
		DODEFAULT()
		
		
	ENDPROC

	PROCEDURE pgfOwnInfo.Page3.cboTerms.Init
		LOCAL lnCount
		
		*
		*  Builds the data displayed in the combo list box
		*
		
		SELECT cTermDesc, cidTerm FROM terms INTO ARRAY laTerms ORDER BY cTermDesc
		lnCount = _tally
		
		This.addlistitem('NONE',1,1)
		This.addlistitem('',1,2)
		
		IF lnCount > 0
		   FOR lnX = 1 TO ALEN(laTerms,1)
		      this.addlistitem(laTerms[lnX,1],lnX+1,1)
		      this.addlistitem(laTerms[lnX,2],lnX+1,2)
		   ENDFOR   
		ENDIF   
		
		this.refresh()
		
		swselect('investor')
		
		DODEFAULT()
	ENDPROC

