	PROCEDURE Init
		LPARAMETERS tcOwner
		
		IF VARTYPE(tcOwner) = 'C'
		   thisform.cOwner = tcOwner
		ELSE
		   thisform.cOwner = '**'
		ENDIF 
		
		IF DODEFAULT()
		   IF thisform.cOwner <> '**'
		      swselect('investor')
		      SEEK(tcOwner)
		   ENDIF
		   
		   IF m.goApp.lDocuments
		      thisform.pgfOwnInfo.page7.Enabled = .t.
		   ELSE
		      thisform.pgfOwnInfo.page7.Enabled = .f.
		   ENDIF 
		ENDIF 
	ENDPROC

	PROCEDURE list
		LPARAMETERS cworkarea
		LOCAL lcList, lnRecNo
		PRIV llOK
		
		llOK = .F.
		lcList = 'cownerid,csortfield'
		
		swselect('investor')
		IF RECC() = 0
		   RETURN
		ENDIF   
		lnRecNo = RECNO()
		
		IF this.flushcontrolbuffer() AND this.beforenav('Investor')
		   DO FORM ..\custom\picklist WITH 'Investor', lcList, thisform.pgfowninfo.page1.txtinvestid.value, 2
		ELSE
		   return   
		ENDIF   
		
		IF llOK
		   swselect('investor')
		   thisform.refresh()
		ELSE
		   swselect('investor')
		   *  Try to go to that record, but if it was a new rec, and they said No about saving it, it won't exist.
		   *  In that case, just refresh whatever record it happens to be on.
		   TRY   
		      GOTO lnRecNo
		   CATCH
		      *  Nada
		   ENDTRY 
		   thisform.refresh()   
		ENDIF   
	ENDPROC

	PROCEDURE Load
		IF DODEFAULT()
		   IF m.goApp.lDocuments
		      thisform.AddObject('oFileDialog','sffiledialog')
		   ENDIF 
		   SET DELETED ON
		ENDIF 
	ENDPROC

	PROCEDURE viewdocument
		LOCAL lnReturn, llReturn, lcOutputFile
		LOCAL liKey, lcPath, llDebug
		
		llDebug = FILE('datafiles\debug.txt')
		
		liKey = INT(VAL(thisform.pgfOwnInfo.page7.lstDocuments.value))
		
		swselect('documents')
		SET ORDER TO iDocPK
		IF SEEK(liKey)
		   lcOutputFile = ALLTRIM(cPath)
		
		   llReturn = .T.
		
		   IF NOT FILE(lcOutputFile)
		      llReturn = .F.
		      IF llDebug
		         MESSAGEBOX('Attempting to view: ' + CHR(10) + ;
		                    lcOutputfile,64,'View Document')
		      ENDIF               
		   ENDIF
		
		   TRY
		      IF llReturn
		         DECLARE INTEGER ShellExecute IN "Shell32.dll" ;
		            INTEGER HWND, ;
		            STRING lpVerb, ;
		            STRING lpFile, ;
		            STRING lpParameters, ;
		            STRING lpDirectory, ;
		            LONG nShowCmd
		
		         lnReturn = ShellExecute(0, "Open", lcOutputFile, "", "", 1)
		         
		      ENDIF
		
		*      THIS.nReturnValue = lnReturn
		
		      IF lnReturn > 32
		         llReturn = .T.
		      ELSE
		         llReturn = .F.
		         IF lldebug
		            MESSAGEBOX('Return from ShellExecute: ' + TRANSFORM(lnReturn),64,'View Document')
		         ENDIF    
		      ENDIF
		   CATCH TO loError
		      IF llDebug
		         MESSAGEBOX('Error Occured: ' + CHR(10) + ;
		                     loError.Message,64,'View Document')
		      ENDIF                
		   ENDTRY
		
		   RETURN (llReturn)
		
		ENDIF
	ENDPROC

	PROCEDURE CMDATAMANAGER.afternav
		LPARAMETERS cworkarea
		
		IF investor.lbackwith  &&  Enabling or disabling the backup withholding tax boxes as necessary
		   THISFORM.pgfOwnInfo.page4.txtnbackpct2.lenabled = .T.
		ELSE
		   THISFORM.pgfOwnInfo.page4.txtnbackpct2.lenabled = .F.
		ENDIF
		
		IF m.goapp.lQbVersion  &&  Don't show finance charge stuff if it's the QB version
		   THISFORM.pgfOwnInfo.Page3.lblCfincode.VISIBLE  = .F.
		   THISFORM.pgfOwnInfo.Page3.cboFinCharge.VISIBLE = .F.
		ENDIF
		
		IF m.goapp.lDirDMDep
		   IF investor.ldirectdep
		      THISFORM.pgfOwnInfo.page6.txtcbanktransit.SET('lENABLED', .T.)
		      THISFORM.pgfOwnInfo.page6.txtcbankacct.SET('lENABLED', .T.)
		      THISFORM.pgfOwnInfo.page6.cbocomboboxcustom1.SET('lENABLED', .T.)
		      THISFORM.pgfOwnInfo.page6.txtbankName.SET('lENABLED', .T.)
		   ELSE
		      THISFORM.pgfOwnInfo.page6.txtcbanktransit.SET('lENABLED', .F.)
		      THISFORM.pgfOwnInfo.page6.txtcbankacct.SET('lENABLED', .F.)
		      THISFORM.pgfOwnInfo.page6.cbocomboboxcustom1.SET('lENABLED', .F.)
		      THISFORM.pgfOwnInfo.page6.txtbankName.SET('lENABLED', .F.)
		   ENDIF
		
		   IF investor.lfedwire
		      THISFORM.pgfOwnInfo.page6.txtFundCode.SET('lENABLED', .T.)
		      THISFORM.pgfOwnInfo.page6.txtpad.SET('lenabled', .T.)
		      THISFORM.pgfOwnInfo.page6.txtPayor.SET('lenabled', .T.)
		      THISFORM.pgfOwnInfo.page6.cboFedIndian.SET('lenabled', .T.)
		   ELSE
		      THISFORM.pgfOwnInfo.page6.txtFundCode.SET('lENABLED', .F.)
		      THISFORM.pgfOwnInfo.page6.txtpad.SET('lenabled', .F.)
		      THISFORM.pgfOwnInfo.page6.txtPayor.SET('lenabled', .F.)
		      THISFORM.pgfOwnInfo.page6.cboFedIndian.SET('lenabled', .F.)
		   ENDIF
		ENDIF
		
		IF investor.nStmtType = 0 AND NOT EMPTY(investor.cownerid)
		   REPLACE investor.nStmtType WITH 1
		   swselect('investor')
		   = TABLEUPDATE(.T.)
		ENDIF
		
		THISFORM.pgfOwnInfo.page7.lstDocuments.REQUERY()
		
		DODEFAULT(cworkarea)
		
		
		
	ENDPROC

	PROCEDURE CMDATAMANAGER.afternew
		LPARAMETERS cworkarea
		
		IF m.cworkarea = 'Investor'
		   REPL lInv1099 WITH .T., ;
		      cfincode WITH '', ;
		      lprtdef  WITH .T., ;
		      ndisbfreq WITH 1,  ;
		      dadded    WITH DATETIME(), ;
		      caddedby  WITH m.goapp.cuser
		
		   THISFORM.pgfOwnInfo.page4.txtnbackpct2.lenabled = .F.  &&  Disable Backup withholding textbox
		   THISFORM.pgfOwnInfo.page4.txtnbackpct2.REFRESH()
		   THISFORM.SetNextControl(THISFORM.pgfOwnInfo.Page1.txtInvestid)
		ENDIF
		
		
	ENDPROC

	PROCEDURE CMDATAMANAGER.afterupdate
		LPARAMETERS cWorkareaList, lCurrentRecordOnly
		
		SELE investor
		REPL dchanged WITH DATETIME(), ;
		   cchangedby WITH m.goapp.cuser
		
		IF 'DUMMY' $ UPPER(cownname)
		   REPLACE ldummy WITH .T.
		ENDIF
		
		IF ldummy
		   *  Set the owner to monthly, no minimum check amount, and not on hold, so no suspense gets created.
		   *  And not to get a 1099, just for good measure.
		   REPLACE ndisbfreq WITH 1, linv1099 WITH .F., lHold WITH .F., nInvMin WITH 0
		   
		   *  Take any interests off hold for this owner
		   swselect('wellinv')
		   SCAN FOR cOwnerID == investor.cOwnerID AND lOnHold
		      REPLACE lOnHold WITH .F.
		   ENDSCAN
		   =TABLEUPDATE(.T.)
		ENDIF
		
		swselect('investor')
		=TABLEUPDATE(.T.)
		
		*  Crummy kludge to fix refresh issue on page 6 if it's active when the user saves
		IF thisform.pgfOwnInfo.ActivePage = 6
		   thisform.pgfOwnInfo.Page7.Activate()
		ENDIF 
		
		RETURN(DODEFAULT(cWorkareaList,lCurrentRecordOnly))
		
	ENDPROC

	PROCEDURE CMDATAMANAGER.beforedelete
		LPARAMETERS cworkarea
		
		IF NOT USED('wellinv')
		   USE wellinv IN 0
		ENDIF
		
		swselect('wellinv')
		LOCATE FOR cownerid = investor.cownerid AND NOT DELETED()
		IF FOUND()
		   THISFORM.oMessage.Warning('This owner has division of interests set up for them.  They cannot be deleted.')
		   RETURN .F.
		ENDIF
		
		IF NOT USED('disbhist')
		   USE disbhist IN 0
		ENDIF
		
		swselect('disbhist')
		LOCATE FOR cownerid = investor.cownerid AND NOT DELETED()
		IF FOUND()
		   THISFORM.oMessage.Warning('This owner has owner history.  They cannot be deleted.')
		   RETURN .F.
		ENDIF
		
		DODEFAULT(cWorkArea)
		
	ENDPROC

	PROCEDURE CMDATAMANAGER.beforeupdate
		LPARAMETERS cWorkareaList, lCurrentRecordOnly
		
		IF NOT EMPTY(investor.cOwnerID)
		   IF THISFORM.cmdatamanager.isnew('Investor')  &&  Only check this on new records, so it doesn't complain on updates to existing ones
		      swselect('vendor')
		      LOCATE FOR cVendorID == THISFORM.pgfOwnInfo.page1.txtInvestid.VALUE
		
		      IF FOUND()  &&  Don't let them have duplicate IDs
		         THISFORM.oMessage.Warning('There is already a vendor defined with this same ID.  To avoid reporting errors, you cannot use the same ID for an owner.')
		         THISFORM.setnextcontrol(THISFORM.pgfOwnInfo.page1.txtInvestid)
		         RETURN .F.
		      ENDIF
		
		      IF NOT m.goapp.lQBVersion  &&  Custs isn't part of the QB database, so we have to use it before checking for duplicates in the AM and DM
		         IF NOT USED('custs')
		            USE custs IN 0
		         ENDIF
		         swselect('custs')
		         LOCATE FOR cCustID == THISFORM.pgfOwnInfo.page1.txtInvestid.VALUE
		         IF FOUND()  &&  Don't let them have duplicate IDs
		            THISFORM.oMessage.Warning('There is already a customer defined with this same ID.  To avoid reporting errors, you cannot use the same ID for an owner.')
		            THISFORM.setnextcontrol(THISFORM.pgfOwnInfo.page1.txtInvestid)
		            RETURN .F.
		         ENDIF
		      ENDIF
		
		      IF investor.linteggl
		         swselect('wellinv')
		         LOCATE FOR cOwnerID == THISFORM.pgfOwnInfo.page1.txtInvestid.VALUE AND cdirect <> 'N'
		         IF FOUND()
		            THISFORM.oMessage.Warning('This owner is marked as direct paid in the division of interests. ' + ;
		               'A direct paid owner cannot be marked to post to the G/L.')
		            RETURN .F.
		         ENDIF
		      ENDIF
		
		      SELECT investor1
		      LOCATE FOR ALLTRIM(UPPER(THISFORM.pgfOwnInfo.page1.txtcownerName.VALUE)) $ UPPER(cOwnName) ;
		         AND cOwnerID <> THISFORM.csaveowner AND THISFORM.csaveowner <> ' '  &&  Check for duplicate name, and give them a warning about possibly screwing up reports
		      IF FOUND()
		         THISFORM.oMessage.Warning('The name you have assigned this owner already exists as part of another owner name.  Be aware that duplicate names can cause reporting errors when reports are sorted by name.')
		      ENDIF
		   ENDIF
		ELSE
		   RETURN .F.
		ENDIF
		
		IF investor.ldirectdep AND (EMPTY(investor.cbankacct) OR EMPTY(investor.cbanktransit))
		   THISFORM.oMessage.Warning('For direct deposit to function, the bank account and bank transit info must be specified.')
		   RETURN .F.
		ENDIF
		
	ENDPROC

