	PROCEDURE pgfOwnInfo.Page7.lstDocuments.Requery
		LPARAMETERS tcType
		
		IF VARTYPE(tcType) # 'C'
		   tcType = 'Desc'
		ENDIF
		
		swselect('options')
		llDocShare = options.lDocShare
		
		THIS.CLEAR()
		lnx = 1
		swselect('documents')
		
		
		IF tcType = 'Desc'
		   SCAN FOR cDocLink = 'OI' AND cFK == investor.cownerid
		      SCATTER MEMVAR
		      THIS.ADDLISTITEM(documents.cdesc, lnx, 1)
		      THIS.ADDLISTITEM(TRANSFORM(documents.idocpk), lnx, 2)
		      lnx = lnx + 1
		   ENDSCAN
		ELSE
		   SCAN FOR cDocLink = 'OI' AND cFK == investor.cownerid
		      SCATTER MEMVAR
		      THIS.ADDLISTITEM(documents.cpath, lnx, 1)
		      THIS.ADDLISTITEM(TRANSFORM(documents.idocpk), lnx, 2)
		      lnx = lnx + 1
		   ENDSCAN
		ENDIF
		
		
		DODEFAULT()
	ENDPROC

