	PROCEDURE pgfOwnInfo.Page1.chkDummy.Click
		IF this.Value
		   swselect('disbhist')
		   SET ORDER to cownerid
		   IF SEEK(investor.cownerid)
		      MESSAGEBOX('This owner cannot be marked as a dummy owner. There are owner history records for this owner ID which means that ' + ;
		                 'this owner has been processed as a NON-DUMMY owner in past runs. Once an owner has been processed they cannot be ' + ;
		                 'marked as a dummy owner.',16,'Dummy Owner')
		      this.set('value',.f.)
		   ENDIF
		   *  Set the owner to monthly, no minimum check amount, and not on hold, so no suspense gets created.
		   *  And not to get a 1099, just for good measure.
		   swselect('investor')
		   replace ndisbfreq WITH 1, linv1099 WITH .f., lHold WITH .f., nInvMin WITH 0, linteggl WITH .f., lexempt WITH .f.
		ENDIF 
	ENDPROC

	PROCEDURE pgfOwnInfo.Page1.cmdWells.Click
		LOCAL lcOwnerID
		
		lcOwnerID = investor.cOwnerID
		
		SELECT wellinv.cwellid,;
		       wells.cwellname,;
		       ctypeinv,ctypeint,;
		       IIF(lOnHold,'Y','N') as cHold,;
		       cwellstat, ;
		       nRevOil, ;
		       nRevGas, ;
		       nWorkint  ;
		   FROM wells,wellinv  ;
		   WHERE wells.cwellid == wellinv.cWellID  ;
		   AND wellinv.cOwnerID == lcOwnerID  ;
		   INTO CURSOR lookuptemp  ;
		   ORDER BY wellinv.cWellID 
		   
		DO FORM dmownerwell LINKED NAME ownerwell
		
		RELE ownerwell
		
		
	ENDPROC

	PROCEDURE pgfOwnInfo.Page1.txtcOwnerName.LostFocus
		IF EMPTY(THISFORM.pgfowninfo.page1.txtsortfield.VALUE) AND NOT EMPTY(THIS.VALUE)
		   IF ' ' $ ALLTRIM(THIS.VALUE)
		      IF THISFORM.omessage.CONFIRM('Do you want to reverse this name for the sort field?')
		         lnx = RAT(' ',ALLTRIM(THIS.VALUE))
		
		         lcLast  = ALLTRIM(SUBSTR(THIS.VALUE,lnX))
		         lcFirst = LEFT(THIS.VALUE,lnX-1)
		
		         m.cSortField = lcLast + ', ' + lcFirst
		         THISFORM.cmdatamanager.REPLACE('Investor','Csortfield',m.cSortField)
		         THISFORM.lchanged = .T.
		         THISFORM.pgfowninfo.page1.txtSortField.REFRESH()
		      ELSE
		         THISFORM.cmdatamanager.REPLACE('Investor','Csortfield',THIS.VALUE)
		         THISFORM.lchanged = .T.
		         THISFORM.pgfowninfo.page1.txtSortField.REFRESH()
		      ENDIF
		   ELSE
		      THISFORM.cmdatamanager.REPLACE('Investor','Csortfield',THIS.VALUE)
		      THISFORM.lchanged = .T.
		      THISFORM.pgfowninfo.page1.txtSortField.REFRESH()
		   ENDIF
		ENDIF
		
		DODEFAULT()
	ENDPROC

	PROCEDURE pgfOwnInfo.Page1.txtcOwnerName.Valid
		lcOwnName = investor.cOwnName
		
		DODEFAULT()
		
		IF investor.cOwnName <> lcOwnName AND NOT EMPTY(lcOwnName)  &&  User changed owner name, so ask about re-doing the sortfield
		   IF THISFORM.oMessage.CONFIRM("You have changed this owner's name.  Would you like to update the sortfield, too?")
		      IF ' ' $ ALLTRIM(THIS.VALUE)
		         IF THISFORM.oMessage.CONFIRM('Do you want to reverse this name for the sort field?')
		            lnx = RAT(' ',ALLTRIM(THIS.VALUE))
		
		            lcLast  = ALLTRIM(SUBSTR(THIS.VALUE,lnx))
		            lcFirst = LEFT(THIS.VALUE,lnx-1)
		
		            m.cSortField = lcLast + ', ' + lcFirst
		            THISFORM.cmdatamanager.REPLACE('Investor','Csortfield',m.cSortField)
		            THISFORM.lchanged = .T.
		            THISFORM.pgfowninfo.page1.txtSortField.REFRESH()
		         ELSE
		            THISFORM.cmdatamanager.REPLACE('Investor','Csortfield',THIS.VALUE)
		            THISFORM.lchanged = .T.
		            THISFORM.pgfowninfo.page1.txtSortField.REFRESH()
		         ENDIF
		      ELSE
		         THISFORM.cmdatamanager.REPLACE('Investor','Csortfield',THIS.VALUE)
		         THISFORM.lchanged = .T.
		         THISFORM.pgfowninfo.page1.txtSortField.REFRESH()
		      ENDIF
		   ENDIF
		ENDIF
		
	ENDPROC

	PROCEDURE pgfOwnInfo.Page1.txtInvestid.Valid
		Local lcOwnerID
		
		lcOwnerID = This.Value
		
		If Not Empty(lcOwnerID)
		    If lcOwnerID <> investor.cownerid
		        Select investor1
		        Set Deleted Off
		        Set Order To cownerid
		        If Seek(lcOwnerID)
		            If Not Deleted()
		                If Thisform.omessage.Confirm('There is already an owner with this ID.  Would you like to view the existing record?')
		                    Thisform.cmdatamanager.revertall()
		                    swselect('investor')
		                    Set Order To cownerid
		                    Seek(lcOwnerID)
		                    Thisform.Refresh()
		                Else
		                    This.Value = ''
		                    Return 0
		                Endif
		            Else
		                Replace investor1.cownerid With Thisform.oRegistry.incrementcounter('%Shared.Counters.DeletedKey')
		                Thisform.Refresh()
		            Endif
		        Endif
		        Set Deleted On
		        swselect('investor')
		    Endif
		Else
		    If Thisform.isnew('Investor') And Empty(lcOwnerID)
		*        Thisform.omessage.warning('Please enter a valid 1-10 character owner id for this owner.')
		*        Return 0
		    Endif
		Endif
		
		DoDefault()
		
	ENDPROC

