	PROCEDURE CMDATAMANAGER.beforeupdate
		LPARAMETERS cWorkareaList, lCurrentRecordOnly
		
		IF suspense.nNetCheck = 0
		   THISFORM.oMessage.Warning('You cannot save a zero amount entry.  Please enter revenue, expenses, or taxes into this record.')
		   RETURN .F.
		ENDIF
		
		tcYear          = THISFORM.runcombo1.LISTITEM[thisform.runcombo1.listitemid, 2]
		tnRunNo         = INT(VAL(THISFORM.runcombo1.LISTITEM[thisform.runcombo1.listitemid, 3]))
		tcGroup         = THISFORM.runcombo1.LISTITEM[thisform.runcombo1.listitemid, 4]
		tcDate          = THISFORM.runcombo1.LISTITEM[thisform.runcombo1.listitemid, 7]
		
		lcOwner = thisform.txtcownerID.Value
		lcWell  = thisform.txtcwellID.Value 
		lcType  = thisform.cboownType.Value 
		
		swselect('wellinv')
		LOCATE FOR cownerid = lcOwner ;
		       AND cwellid  = lcWell ;
		       AND ctypeinv = lcType
		IF FOUND() AND wellinv.lJIB
		   IF thisform.pgfpageframecustom1.pAGE1.txtexpense.Value # 0 OR ;
		      thisform.pgfpageframecustom1.pAGE1.txttotale1.Value # 0 OR ;
		      thisform.pgfpageframecustom1.pAGE1.txttotale2.Value # 0 OR ;
		      thisform.pgfpageframecustom1.pAGE1.txttotale3.Value # 0 OR ;
		      thisform.pgfpageframecustom1.pAGE1.txttotale4.Value # 0 OR ;
		      thisform.pgfpageframecustom1.pAGE1.txttotale5.Value # 0
		      IF MESSAGEBOX("This owner's interest is a JIB interest. " + ;
		                   "Any expenses entered will not be processed but they will affect the 1099 for this owner. " + ;
		                   "Are you sure you want to do this?",36,'Suspense Balances') = 7
		         RETURN .f. 
		      ENDIF    
		   ENDIF    
		ENDIF 
		   
		IF EMPTY(tcYear) OR ;
		   tnRunno = 0  
		   MESSAGEBOX('You must pick a run number to assign this suspense to.',16,'Missing Run No')
		   RETURN .f.
		ENDIF 
		
		SELECT suspense   
		replace crunyear    WITH tcYear
		replace nrunno      WITH tnRunNo
		replace crunyear_in WITH tcYear
		replace nrunno_in   WITH tnRunNo
		replace cGroup      WITH tcGroup
		replace cRecType    WITH 'R'
		replace hdate    WITH CTOD(tcDate)
		
		swselect('investor')
		LOCATE FOR cownerid == suspense.cownerid
		IF FOUND()
		   *  Don't allow entries to be made for owners who are set to post
		   IF lIntegGL
		      DO CASE
		         CASE m.goapp.lAMVersion
		            THISFORM.oMessage.Warning('This owner is set to be posted to the GL instead of creating a check.  A suspense balance cannot be entered for them.')
		            RETURN .F.
		         CASE m.goapp.lQBVersion
		            THISFORM.oMessage.Warning('This owner is set to be posted to QB instead of creating a check.  A suspense balance cannot be entered for them.')
		            RETURN .F.
		      ENDCASE
		   ENDIF
		ENDIF
		
		DODEFAULT(cWorkareaList, lCurrentRecordOnly)
		
	ENDPROC

