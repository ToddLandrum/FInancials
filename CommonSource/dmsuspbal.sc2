	PROCEDURE Init
		IF DODEFAULT()
		   IF NOT m.goapp.lAMVersion
		      THISFORM.label1.CAPTION = '**  The information entered here will affect the NEXT run closing that is done for this well.  Entries made here will also affect 1099 amounts.  **'
		      THISFORM.label2.VISIBLE = .F.
		   ELSE
		      THISFORM.label1.VISIBLE = .T.
		      THISFORM.label2.VISIBLE = .T.
		   ENDIF
		
		* Only look at Manually entered records with a cRecType of "R"
		* Those would have been entered in this screen.
		   SWSELECT('suspense')
		   GO BOTT
		
		*!*   thisform.SetState()
		*!*   thisform.prior()
		*!*   thisform.last()
		ENDIF
		
	ENDPROC

	PROCEDURE list
		LPARAMETERS cworkarea
		
		LOCAL lcList
		PRIV llOK
		
		llOK = .F.
		
		IF THIS.flushcontrolbuffer() AND THIS.beforenav('suspense')
		   IF THISFORM.omessage.CONFIRM('Should the lookup list include the owner name?. Including the owner name will take longer to bring the list up.')
		      lcList = 'cownerid\Owner ID,csortfield\Owner Name,cwellid\Well ID,cwellname\Well Name,hyear\Year,hperiod\Prd,ctypeinv\Type,nNetCheck\Net Check'
		      IF NOT USED('tempohist')
		         SELECT suspense.cownerid, ;
		            investor.csortfield, ;
		            suspense.cwellid, ;
		            wells.cwellname, ;
		            suspense.hyear, ;
		            suspense.hperiod, ;
		            suspense.ctypeinv, ;
		            suspense.nNetCheck,  ;
		            suspense.ciddisb ;
		            FROM suspense JOIN investor ON suspense.cownerid = investor.cownerid ;
		                          JOIN wells    ON suspense.cwellid  = wells.cwellid ;
		            WHERE suspense.lManual  ;
		            AND suspense.cRecType <> 'P'  ;
		            INTO CURSOR tempohist READWRITE 
		      ENDIF
		
		      IF _TALLY > 0
		            SET SAFETY off
		            SELECT tempohist
		            INDEX ON cownerid+hyear+hperiod+cwellid+ctypeinv TAG cownerid
		            INDEX ON csortfield+cwellid+hyear+hperiod+ctypeinv TAG cownname
		            INDEX ON cwellid+hyear+hperiod+cownerid+ctypeinv TAG cwellid
		            INDEX ON cwellname+hyear+hperiod+cownerid+ctypeinv TAG cwellname
		            INDEX ON hyear+hperiod+cownerid+cwellid+ctypeinv TAG hyear
		            INDEX ON hyear+hperiod+cownerid+cwellid+ctypeinv TAG hperiod
		            INDEX on nNetCheck TAG nNetCheck
		
		         DO FORM ..\CUSTOM\picklist WITH 'tempohist', lcList, THISFORM.txtcownerid.VALUE, 8, .T.,.t.
		
		         IF llOK
		            swselect('suspense')
		            SET ORDER TO ciddisb
		            SEEK(tempohist.ciddisb)
		            THISFORM.REFRESH()
		            THISFORM.refreshinfo()
		         ENDIF
		      ENDIF
		   ELSE
		      lcList = 'cownerid,cwellid,hyear,hperiod,ctypeinv,nnetcheck'
		      DO FORM ..\CUSTOM\picklist WITH 'suspense', lcList, THISFORM.txtcownerid.VALUE, 6
		
		         IF llOK
		            THISFORM.REFRESH()
		            THISFORM.refreshinfo()
		         ENDIF
		   ENDIF
		  
		ENDIF
		
	ENDPROC

