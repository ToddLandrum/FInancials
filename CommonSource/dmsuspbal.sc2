	PROCEDURE txtcOwnerID.Valid
		IF THISFORM.isnew()  &&  If adding a new record, fill in the interests automatically
		   swselect('wellinv')
		   LOCATE FOR cwellid = THISFORM.txtcWellID.VALUE AND cownerid = THIS.VALUE
		   IF FOUND()
		      SCATTER MEMVAR
		      m.cRectype = 'R'
		      swselect('wells')
		      LOCATE FOR cwellid = m.cwellid
		      m.cGroup = wells.cGroup
		      m.hDate = DATE()
		      swselect('suspense')
		      GATHER MEMVAR
		      THISFORM.Findinterests()
		      THISFORM.REFRESH()
		   ENDIF
		ENDIF
		DODEFAULT()
		
	ENDPROC

