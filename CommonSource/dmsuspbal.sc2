	PROCEDURE calctot		&& Calculates the total from the data found in each field of the table.
		LOCAL lnAmount
		
		
		lnAmount = suspense.noilrev + suspense.ngasrev + suspense.nothrev + ;
		   suspense.ntrprev + suspense.nmiscrev1 + ;
		   suspense.nmiscrev2 + suspense.nflatrate - ;
		   suspense.noiltax1 - suspense.ngastax1  - ;
		   suspense.noiltax2 - suspense.ngastax2  - ;
		   suspense.noiltax3 - suspense.ngastax3  - ;
		   suspense.noiltax4 - suspense.ngastax4  - ;
		   suspense.nothtax1 - suspense.nothtax2  - ;
		   suspense.nothtax3 - suspense.nothtax4  - ;
		   suspense.nBackWith - suspense.nTaxWith -  ;
		   (suspense.nExpense + suspense.nTotale1 + ;
		   suspense.nTotale2 + suspense.nTotale3 + ;
		   suspense.nTotale4 + suspense.nTotale5 + ;
		   suspense.nTotaleA + suspense.nTotaleB + ;
		   suspense.ngather  + suspense.ncompress)
		
		IF suspense.nIncome <> (suspense.noilrev + suspense.ngasrev + suspense.nothrev + ;
		      suspense.ntrprev + suspense.nmiscrev1 + suspense.nmiscrev2 + suspense.nflatrate)
		   REPLACE suspense.nIncome WITH (suspense.noilrev + suspense.ngasrev + suspense.nothrev + ;
		      suspense.ntrprev + suspense.nmiscrev1 + suspense.nmiscrev2 + suspense.nflatrate)
		ENDIF
		IF suspense.nSevTaxes <> (suspense.noiltax1 + suspense.ngastax1 + ;
		      suspense.noiltax2 + suspense.ngastax2 +  suspense.noiltax3 + suspense.ngastax3  + ;
		      suspense.noiltax4 + suspense.ngastax4 + suspense.nothtax1 + suspense.nothtax2  + ;
		      suspense.nothtax3 + suspense.nothtax4)
		   REPLACE suspense.nSevTaxes WITH (suspense.noiltax1 + suspense.ngastax1 + ;
		      suspense.noiltax2 + suspense.ngastax2 +  suspense.noiltax3 + suspense.ngastax3  + ;
		      suspense.noiltax4 + suspense.ngastax4 + suspense.nothtax1 + suspense.nothtax2  + ;
		      suspense.nothtax3 + suspense.nothtax4)
		ENDIF
		
		IF suspense.nnetcheck <> lnAmount
		   THISFORM.cmdatamanager.REPLACE('suspense', 'nNetcheck', lnAmount)
		   THISFORM.txtnNetCheck.REFRESH()
		ENDIF
		
	ENDPROC

