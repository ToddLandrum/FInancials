	PROCEDURE refreshinfo
		THISFORM.txtcWellID.REFRESH()
		    THISFORM.txtcOwnerID.REFRESH()
		
		    IF suspense.ctypeInv <> 'W'
		        THISFORM.pgfpageframecustom1.page1.txtexpense.SET('enabled', .F.)
		        THISFORM.pgfpageframecustom1.page3.txtWorkint.SET('enabled', .F.)
		    ELSE
		        THISFORM.pgfpageframecustom1.page1.txtexpense.SET('enabled', .T.)
		        THISFORM.pgfpageframecustom1.page3.txtWorkint.SET('enabled', .T.)
		    ENDIF
		
		    IF NOT EMPTY(THISFORM.txtcWellID.VALUE)
		        swselect('wells')
		        SET ORDER TO cWellID
		        IF SEEK(THISFORM.txtcWellID.VALUE)
		            THISFORM.txtcWellName.VALUE = wells.cWellName
		            THISFORM.txtcWellName.REFRESH()
		        ELSE
		            THISFORM.txtcWellName.VALUE = ''
		            THISFORM.txtcWellName.REFRESH()
		        ENDIF
		    ENDIF
		
		    IF NOT EMPTY(THISFORM.txtcOwnerID.VALUE)
		        swselect('investor')
		        SET ORDER TO cOwnerID
		        IF SEEK(THISFORM.txtcOwnerID.VALUE)
		            THISFORM.txtcOwnerName.VALUE = investor.cOwnname
		            THISFORM.txtcOwnerName.REFRESH()
		        ELSE
		            THISFORM.txtcOwnerName.VALUE = ''
		            THISFORM.txtcOwnerName.REFRESH()
		        ENDIF
		    ENDIF
		
		    THISFORM.runcombo1.REQUERY()
		    FOR lnx = 1 TO THISFORM.runcombo1.LISTCOUNT
		        IF THISFORM.runcombo1.LISTITEM[lnx, 2] = suspense.cRunYear AND ;
		                INT(VAL(THISFORM.runcombo1.LISTITEM[lnx, 3])) = suspense.nRunNo AND ;
		                THISFORM.runcombo1.LISTITEM[lnx, 4] = suspense.cgroup AND ;
		                THISFORM.runcombo1.LISTITEM[lnx, 6] = suspense.crectype
		            THISFORM.runcombo1.LISTITEMID = lnx
		            EXIT
		        ENDIF
		    ENDFOR
	ENDPROC

