	PROCEDURE Activate
		DODEFAULT()
		
		IF tax1099det.cIDType <> 'I'
		   THISFORM.lblcrptGroup.VISIBLE = .F.
		   THISFORM.cbocRptGroup.VISIBLE = .F.
		ELSE
		   THISFORM.lblcrptGroup.VISIBLE = .T.
		   THISFORM.cbocRptGroup.VISIBLE = .T.
		ENDIF
		
		
	ENDPROC

	PROCEDURE calcnet		&& Calculates the net amount at the bottom by taking gross revenue and subtracting off everything else.
		LOCAL lnNet
		
		lnNet = thisform.txtnIncome.Value - (thisform.txtnexpense.Value + thisform.txtnsevTaxes.Value +  ;
		        thisform.txtfedTax.Value + thisform.txtstatetax.Value + thisform.txtncompGath.value)
		
		IF tax1099det.nNetCheck <> lnNet
		   thisform.txtnnetCheck.set('value',lnNet)
		ENDIF 
	ENDPROC

	PROCEDURE Init
		
		IF DODEFAULT()
		   swselect('tax1099det')
		   ten99year = thisform.cboyear.DisplayValue
		   SET FILTER TO cyear == ten99year
		   GO TOP                 
		   thisform.Refresh()             
		   thisform.lShowWarning = .T.
		ENDIF 
	ENDPROC

	PROCEDURE list
		LPARAMETERS cworkarea
		
		llOK = .T.
		
		lcList = 'cyear\Year,cid\ID,cname\Name,cwellid\Well ID,cwellname\Well Name'
		
		SELECT cYear,cID,cName,cWellID,cWellName,cidtax1 FROM tax1099det WHERE cyear == ten99year INTO CURSOR temp readwrite
		
		SELECT temp
		INDEX on cyear+cid TAG cYear
		INDEX on cid+cwellid TAG cID
		INDEX on cName+cwellid TAG cName
		INDEX on cWellID+cID TAG cWellID
		INDEX on cWellName+cID TAG cWellName
		
		DO FORM ..\CUSTOM\picklist WITH 'temp', lcList,tax1099det.cid, 5, .t., .t.
		
		IF llok  &&  If an owner/vendor was selected
		   SELECT tax1099det
		   LOCATE FOR cidtax1 = temp.cidtax1
		   THISFORM.REFRESH()
		ENDIF
		
		thisform.setnextcontrol(thisform.swyear1)
	ENDPROC

	PROCEDURE Load
		PUBLIC ten99year
		DODEFAULT()
	ENDPROC

	PROCEDURE cbocRptGroup.Init
		this.addlistitem('Royalty',1,1)
		this.addlistitem('R',1,2)
		this.addlistitem('Working',2,1)
		this.addlistitem('W',2,2)
		
		dodefault()
		
	ENDPROC

	PROCEDURE cboType.Init
		this.addlistitem('Owner',1,1)
		this.addlistitem('I',1,2)
		this.addlistitem('Vendor',2,1)
		this.addlistitem('V',2,2)
		
		dodefault()
		
	ENDPROC

	PROCEDURE cboYear.Init
		IF DODEFAULT()
		   SELECT cyear FROM tax1099det INTO CURSOR temp ORDER BY cyear DESC GROUP BY cyear
		   lnkey = 1
		   SELECT temp
		   SCAN
		      SCATTER memvar
		      this.AddListItem(m.cyear,lnkey)
		      lnkey = lnkey + 1
		   ENDSCAN 
		   ten99year = this.DisplayValue 
		   this.ListItemId = 1
		   SELECT tax1099det
		   SET FILTER TO cyear = ten99year
		ENDIF 
	ENDPROC

	PROCEDURE cboYear.InteractiveChange
		swselect('tax1099det')
		ten99year = this.DisplayValue
		SET FILTER TO cyear = ten99year
		GO TOP
		thisform.Refresh() 
	ENDPROC

	PROCEDURE CMDATAMANAGER.afternav
		LPARAMETERS cWorkarea
		
		*  Make the Owner Type dropdown invisible if it's a vendor record
		IF m.cWorkarea = 'Tax1099det'
		   IF tax1099det.cIDType <> 'I'
		      thisform.lblcrptGroup.Visible = .f.
		      thisform.cbocRptGroup.Visible = .f.
		   ELSE
		      thisform.lblcrptGroup.Visible = .t.
		      thisform.cbocRptGroup.Visible = .t.
		   ENDIF
		ENDIF
		
		DODEFAULT(cWorkarea)
	ENDPROC

	PROCEDURE CMDATAMANAGER.afternew
		LPARAMETERS cworkarea
		
		m.cidtax1 = THISFORM.oRegistry.IncrementCounter('%Shared.Counters.1099Key')
		
		SELECT tax1099det1
		SET ORDER TO cidtax1
		SET DELE OFF
		DO WHILE SEEK(m.cidtax1)
		   m.cidtax1 = THISFORM.oRegistry.IncrementCounter('%Shared.Counters.1099Key')
		ENDDO
		
		SELECT tax1099det
		SET DELE ON
		
		REPLACE tax1099det.cidtax1 WITH m.cidtax1
		REPLACE tax1099det.cidtype WITH 'I'
		
		*!*	IF MONTH(DATE()) < 7  &&  Still the first half of the year, so make the year it fills in be last year; otherwise, use the current year
		*!*	   REPLACE tax1099det.cYear WITH ALLTRIM(STR(YEAR(DATE())-1))
		*!*	ELSE
		*!*	   REPLACE tax1099det.cYear WITH ALLTRIM(STR(YEAR(DATE())))
		*!*	ENDIF
		
		* Fill in the year from the filter
		REPLACE tax1099det.cYear WITH ten99year
		
		IF options.lGross1099  &&  If building 1099s on gross, mark this as gross
		   REPLACE tax1099det.cType WITH 'G'
		ELSE
		   REPLACE tax1099det.cType WITH 'N'
		ENDIF
		
		THISFORM.setnextcontrol(THISFORM.swyear1)
		
		DODEFAULT(cworkarea)
		
	ENDPROC

	PROCEDURE CMDATAMANAGER.afterupdate
		LPARAMETERS cWorkareaList, lCurrentRecordOnly
		
		IF thisform.lShowWarning = .T. 
		    MESSAGEBOX("Any changes made to this window will not be reflected on either the 'Update 1099 File' " + ;
		               "or 'Update State 1099 File' windows." + CHR(10)+CHR(10) + ;  
		               "You need to manually update both of those windows with the same changes so that the " + ;
		               "reconciliation report matches what is reported to the IRS and state.",48,'Detail File Update')
		   thisform.lShowWarning = .F.
		ENDIF 
		
		DODEFAULT(cWorkareaList, lCurrentRecordOnly)
	ENDPROC

	PROCEDURE cmdLookID.Click
		llOK = .T.
		
		lcList = 'cid\ID,cname\Name,ctype\Type'
		SET SAFETY OFF
		SELECT custownvend
		SET FILTER TO ctype = 'O'
		INDEX ON cid TAG cid
		INDEX ON cname TAG cname
		INDEX ON ctype TAG ctype
		
		DO FORM ..\CUSTOM\picklist WITH 'custownvend', lcList,tax1099det.cid, 3, .f., .t.
		
		sele custownvend
		dele tag all
		
		IF llok  &&  If an owner/vendor was selected
		   THISFORM.cmdatamanager.REPLACE('tax1099det','Cid',custownvend.cid)
		   THISFORM.txtid.VALUE = custownvend.cid
		   THISFORM.setnextcontrol(THISFORM.txtid)
		ELSE
		   RETURN .F.
		ENDIF
		
	ENDPROC

	PROCEDURE cmdWellID.Click
		LOCAL lcList
		PRIV llOK, llNew
		
		llOK   = .T.
		lcList = 'cWellid,cwellname,cgroup'
		
		DO FORM ..\custom\picklist WITH 'Wells', lcList, tax1099det.cwellid, 3
		
		IF llOK
		   thisform.txtcwellid.set('value',wells.cwellid)
		   thisform.txtcWellName.set('value',wells.cWellName)
		ENDIF   
		
		thisform.setnextcontrol(thisform.txtcwellid)
	ENDPROC

	PROCEDURE Swyear1.Valid
		IF EMPTY(THIS.VALUE)
		   THIS.SET('VALUE',ALLTRIM(STR(YEAR(DATE()))))
		ENDIF
		
		IF NOT BETWEEN(THIS.VALUE,'1980','2050')
		   THISFORM.oMessage.Warning('An invalid year was entered.  Please enter a valid year before continuing.')
		   RETURN 0
		ENDIF
		
		DODEFAULT()
		
	ENDPROC

