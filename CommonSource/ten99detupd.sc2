	PROCEDURE txtId.Valid
		LOCAL llOKToChange
		ON KEY LABEL F2 *
		*
		*  Look Up the owner in the owner file
		*
		llOKToChange = .F.
		
		IF THIS.VALUE <> tax1099det.cid
		   SELE tax1099det1
		   SET ORDER TO cid
		   IF SEEK(THIS.VALUE)
		      m.cidtax1 = cidtax1
		      SELE tax1099det
		      SET ORDER TO cidtax1
		      IF SEEK(m.cidtax1)
		         this.lchanged = .f.
		         thisform.lchanged = .f.
		         THISFORM.REFRESH()
		      ENDIF
		   ELSE
		      llOKToChange = .T.
		   ENDIF
		ELSE
		   llOKToChange = .T.
		ENDIF
		
		IF llOKToChange
		   SELECT investor
		   SET ORDER to cownerid
		   IF SEEK(THIS.VALUE)
		      IF tax1099det.cid <> THIS.VALUE
		         THISFORM.cmdatamanager.REPLACE('tax1099det','cID',THIS.VALUE)
		      ENDIF
		      IF ALLT(tax1099det.cname) <> ALLT(investor.cOwnName)
		         THISFORM.cmdatamanager.REPLACE('tax1099det','cname',investor.cOwnName)
		      ENDIF
		      IF ALLT(tax1099det.cAddr1) <> ALLT(investor.cAddress1a)
		         THISFORM.cmdatamanager.REPLACE('tax1099det','caddr1',investor.cAddress1a)
		      ENDIF
		      IF ALLT(tax1099det.cAddr2) <> ALLT(investor.caddress1b)
		         THISFORM.cmdatamanager.REPLACE('tax1099det','caddr2',investor.caddress1b)
		      ENDIF
		      IF ALLT(tax1099det.cCity) <> ALLT(investor.cCity1)
		         THISFORM.cmdatamanager.REPLACE('tax1099det','ccity',investor.cCity1)
		      ENDIF
		      IF tax1099det.cState <> investor.cState1
		         THISFORM.cmdatamanager.REPLACE('tax1099det','cstate',investor.cState1)
		      ENDIF
		      IF ALLT(tax1099det.cZip) <> ALLT(investor.cZip1)
		         THISFORM.cmdatamanager.REPLACE('tax1099det','czip',investor.cZip1)
		      ENDIF
		      IF ALLT(tax1099det.cTaxID) <> ALLT(investor.cTaxID)
		         THISFORM.cmdatamanager.REPLACE('tax1099det','ctaxid',investor.cTaxID)
		      ENDIF
		      IF tax1099det.cidType <> 'I'
		         THISFORM.cboType.SET('value','I')
		      ENDIF
		      THISFORM.REFRESH()
		   ELSE
		      SELECT vendor
		      SET ORDER to cvendorid
		      IF SEEK(THIS.VALUE)
		         IF tax1099det.cid <> THIS.VALUE
		            THISFORM.cmdatamanager.REPLACE('tax1099det','cID',THIS.VALUE)
		         ENDIF
		         IF ALLT(tax1099det.cname) <> ALLT(vendor.cVendName)
		            THISFORM.cmdatamanager.REPLACE('tax1099det','cname',vendor.cVendName)
		         ENDIF
		         IF ALLT(tax1099det.cAddr1) <> ALLT(vendor.cbAddr1)
		            THISFORM.cmdatamanager.REPLACE('tax1099det','caddr1',vendor.cbAddr1)
		         ENDIF
		         IF ALLT(tax1099det.cAddr2) <> ALLT(vendor.cbAddr2)
		            THISFORM.cmdatamanager.REPLACE('tax1099det','caddr2',vendor.cbAddr2)
		         ENDIF
		         IF ALLT(tax1099det.cCity) <> ALLT(vendor.cbCity)
		            THISFORM.cmdatamanager.REPLACE('tax1099det','ccity',vendor.cbCity)
		         ENDIF
		         IF tax1099det.cState <> vendor.cbState
		            THISFORM.cmdatamanager.REPLACE('tax1099det','cstate',vendor.cbState)
		         ENDIF
		         IF tax1099det.cZip <> vendor.cbZip
		            THISFORM.cmdatamanager.REPLACE('tax1099det','czip',vendor.cbZip)
		         ENDIF
		         IF ALLT(tax1099det.cTaxID) <> ALLT(vendor.cTaxID)
		            THISFORM.cmdatamanager.REPLACE('tax1099det','ctaxid',vendor.cTaxID)
		         ENDIF
		         IF tax1099det.cidType <> 'V'
		            THISFORM.cboType.SET('value','V')
		         ENDIF
		         THISFORM.REFRESH()
		      ELSE
		         THISFORM.cmdatamanager.REPLACE('tax1099det','cID',THIS.VALUE)
		         THISFORM.cmdatamanager.REPLACE('tax1099det','cname','  ')
		         THISFORM.cmdatamanager.REPLACE('tax1099det','caddr1',' ')
		         THISFORM.cmdatamanager.REPLACE('tax1099det','caddr2',' ')
		         THISFORM.cmdatamanager.REPLACE('tax1099det','ccity',' ')
		         THISFORM.cmdatamanager.REPLACE('tax1099det','cstate',' ')
		         THISFORM.cmdatamanager.REPLACE('tax1099det','czip',' ')
		         THISFORM.cmdatamanager.REPLACE('tax1099det','ctaxid',' ')
		         THISFORM.cboType.SET('value','V')
		         THISFORM.REFRESH()
		      ENDIF
		   ENDIF
		ENDIF
		
	ENDPROC

