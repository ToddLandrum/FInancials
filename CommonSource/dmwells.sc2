	PROCEDURE Activate
		IF DODEFAULT()
		   thisform.well_on_hold()
		ENDIF    
	ENDPROC

	PROCEDURE beforeclose
		LPARAMETERS nclosereason
		
		IF THISFORM.lchanged AND NOT thisform.isnew('Wells')
		    IF EMPTY(THISFORM.Pgfpageframe1.Page1.txtWellid.VALUE) OR EMPTY(THISFORM.Pgfpageframe1.Page1.txtWellname.VALUE)
		        THISFORM.oMessage.Warning('You cannot save a well record with a blank name or ID.  Please correct before proceeding.')
		        RETURN .F.
		    ENDIF
		ENDIF
		
		DODEFAULT(nclosereason)
		
	ENDPROC

	PROCEDURE chkrefresh
		WITH THISFORM.pgfPageFrame1.page4
		   IF wells.lcompress
		      .txtnCompress.lenabled = .T.
		   ELSE
		      .txtnCompress.lenabled = .F.
		   ENDIF
		
		   IF wells.lgather
		      .txtnGather.lenabled = .T.
		   ELSE
		      .txtnGather.lenabled = .F.
		   ENDIF
		ENDWITH
	ENDPROC

	PROCEDURE Init
		IF DODEFAULT()
		   IF FILE('datafiles\northwood.txt')
		      thisform.lnorthwood = .t.
		      thisform.pgfpageframe1.page2.lbluser3.Caption = 'Tank #'
		   ENDIF
		   IF FILE('datafiles\bdoil.txt')
		      thisform.pgfpageframe1.page1.lblcpumper.Caption = 'Customer'
		   ENDIF 
		ENDIF
		
	ENDPROC

	PROCEDURE list
		LPARAMETERS cworkarea
		LOCAL lcList, lnRecNo, llDontMove
		PRIV llOK
		
		IF thisform.lnorthwood
		   thisform.listnorthwood()
		   RETURN
		ENDIF 
		
		llOK = .F.
		llDontMove = .F.
		
		lcList = 'cwellid,cwellname,cgroup,cwellstat'
		
		SELECT wells
		lnRecNo = RECNO()
		
		IF thisform.cmdatamanager.isnew('Wells')
		   llDontMove = .T.
		ENDIF 
		      
		IF this.flushcontrolbuffer() AND this.beforenav('Wells')
		   DO FORM custom\picklist WITH 'Wells', lcList, thisform.pgfpageframe1.page1.txtwellid.value, 4
		ELSE
		   RETURN    
		ENDIF   
		
		IF NOT llOK
		   IF NOT llDontMove
		      TRY 
		         SELECT wells
		         GOTO lnRecNo
		      CATCH
		      ENDTRY 
		   ENDIF    
		ELSE
		   THISFORM.cmdatamanager.afternav('Wells')
		ENDIF   
		
		THISFORM.REFRESH()   
		   
		
	ENDPROC

	PROCEDURE listnorthwood
		LPARAMETERS cworkarea
		LOCAL lcList, lnRecNo, llDontMove
		PRIV llOK
		
		llOK = .F.
		llDontMove = .F.
		
		lcList = 'cwellid,cwellname,cpermit1,ccounty,ctownship,cgroup,cwellstat'
		
		SELECT wells
		lnRecNo = RECNO()
		
		IF thisform.cmdatamanager.isnew('Wells')
		   llDontMove = .T.
		ENDIF 
		      
		IF this.flushcontrolbuffer() AND this.beforenav('Wells')
		   DO FORM custom\picklist WITH 'Wells', lcList, thisform.pgfpageframe1.page1.txtwellid.value, 7
		ELSE
		   RETURN    
		ENDIF   
		
		IF NOT llOK
		   IF NOT llDontMove
		      TRY 
