	PROCEDURE Pgfpageframe1.Page5.chkRoysevtx.Click
		IF this.Parent.chklusesev.Value
		   MESSAGEBOX('This option cannot be enabled when the option to calculate taxes using the below rates is enabled.',16,'Conflicting Option')
		   this.Value = .f.
		ENDIF 
	ENDPROC

	PROCEDURE Pgfpageframe1.Page5.chkTaxexempt.Click
		IF wells.lusesev AND this.value
		   IF thisform.omessage.confirm('This well is marked to use the rates specified below. These rates override the tax 1 calculations ' + ;
		                                'for oil & gas. Do you want to continue to mark the well as exempt from tax 1 which will zero out the rates?')
		      thisform.cmdatamanager.replace('Wells','Lusesev',.f.)
		      thisform.cmdatamanager.replace('Wells','nroysevo',0)
		      thisform.cmdatamanager.replace('Wells','nroysevg',0)
		      thisform.cmdatamanager.replace('Wells','nwrksevo',0)
		      thisform.cmdatamanager.replace('Wells','nwrksevg',0)
		      thisform.pgfpageframe1.page5.chkTaxexempt1.Refresh()
		      thisform.pgfpageframe1.page5.txtnroysevo.Refresh()
		      thisform.pgfpageframe1.page5.txtnroysevg.Refresh()
		      thisform.pgfpageframe1.page5.txtnwrksevo.Refresh()
		      thisform.pgfpageframe1.page5.txtnwrksevg.Refresh()
		   ELSE
		      this.Value = .f.
		   ENDIF 
		ENDIF 
		DODEFAULT()
	ENDPROC

