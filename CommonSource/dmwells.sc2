	PROCEDURE Pgfpageframe1.Page4.chkThird_prty.Click
		IF this.value AND NOT EMPTY(wells.cTable)  &&  Don't let them mark this option if they've got a tax table
		   messagebox('You cannot mark this option because this well has a tax table assigned to it, ' + ;
		              'which means you are not liable for paying the taxes being withheld. The original ' + ;
		              'operator will have withheld the taxes before the revenue was received.',16,'Option Conflict')
		   this.set('value',.f.)
		   return
		ENDIF
		
		IF this.Value
		   thisform.cmdatamanager.replace('Wells','lSev1O',.T.)
		   thisform.cmdatamanager.replace('Wells','lSev2O',.T.)
		   thisform.cmdatamanager.replace('Wells','lSev3O',.T.)
		   thisform.cmdatamanager.replace('Wells','lSev4O',.T.)
		   thisform.cmdatamanager.replace('Wells','lSev1G',.T.)
		   thisform.cmdatamanager.replace('Wells','lSev2G',.T.)
		   thisform.cmdatamanager.replace('Wells','lSev3G',.T.)
		   thisform.cmdatamanager.replace('Wells','lSev4G',.T.)
		   thisform.cmdatamanager.replace('Wells','lSev1P',.T.)
		   thisform.cmdatamanager.replace('Wells','lSev2P',.T.)
		   thisform.cmdatamanager.replace('Wells','lSev3P',.T.)
		   thisform.cmdatamanager.replace('Wells','lSev4P',.T.)
		   thisform.cmdatamanager.replace('Wells','lDirOilPurch',.T.)
		   thisform.cmdatamanager.replace('Wells','lDirGasPurch',.T.)
		ELSE
		   thisform.omessage.display('Be sure to check the tax options to make sure they are setup correctly for this well.')
		ENDIF 
		   
		DODEFAULT()
		
	ENDPROC

	PROCEDURE Pgfpageframe1.Page4.cmdHold.Click
		swselect('investor')
		SET ORDER TO cownerid
		swselect('wellinv',.T.)
		IF THIS.CAPTION = 'Suspend Well'
		   IF THISFORM.omessage.CONFIRM('This will mark all owner interests as held in the division of interests for this well. Continue?')
		      SELECT wellinv
		      SCAN FOR cwellid == wells.cwellid
		         SELECT investor
		         IF SEEK(wellinv.cownerid) AND linteggl
		            LOOP
		         ENDIF
		         SELECT wellinv
		         REPLACE lonhold WITH .T.
		      ENDSCAN
		      TABLEUPDATE(.T.)
		      MESSAGEBOX('All interests in this well have been placed on hold. ' + CHR(10) + CHR(10) +;
		         'Changing the division of interests manually by taking ' + CHR(10) + ;
		         'an interest off hold will change this button to say ' + CHR(10) + ;
		         '"Suspend Well" again.',64,'Release Well')
		   ENDIF
		ELSE
		   IF THISFORM.omessage.CONFIRM('This will mark all owner interests as not held in the division of interests for this well. Continue?')
		      SELECT wellinv 
		      SCAN FOR cwellid == wells.cwellid
		         REPLACE lonhold WITH .F.
		      ENDSCAN
		      TABLEUPDATE(.T.)
		      MESSAGEBOX('All interests in this well have been taken off hold. ' + CHR(10) + CHR(10) +;
		         'Changing the division of interests manually by placing ' + CHR(10) + ;
		         'an interest on hold will change this button to say ' + CHR(10) + ;
		         '"Release Well" again.',64,'Release Well')
		   ENDIF
		ENDIF
		
		THISFORM.well_on_hold()
		
	ENDPROC

