	PROCEDURE Pgfpageframe1.Page1.txtWellid.Valid
		LOCAL lcWellID
		
		lcWellID = THIS.VALUE
		
		IF NOT EMPTY(lcWellID) AND lcWellID <> wells.cwellid
		   SELECT wells1
		   SET DELETED OFF
		   SET ORDER TO cwellid
		   IF SEEK(lcWellID)
		      IF NOT DELETED()
		         IF THISFORM.omessage.CONFIRM('There is already a well with this ID.  Would you like to view the existing record?')
		            THISFORM.cmdatamanager.revertall()
		            SELECT wells
		            SET ORDER TO cwellid
		            SEEK(lcWellID)
		            THISFORM.REFRESH()
		         ELSE
		            THIS.VALUE = ''
		            RETURN 0
		         ENDIF
		      ELSE
		         THISFORM.cmdatamanager.revertall()
		         SELECT wells
		         SET ORDER TO cwellid
		         SEEK(lcWellID)
		         SCATTER MEMVAR BLANK MEMO
		         GATHER MEMVAR MEMO
		         RECALL
		         THIS.SET('value',lcWellID)
		         REPLACE nprocess    WITH 1, ;
		            cwellstat   WITH 'A', ;
		            cGroup      WITH '00'
		         THISFORM.REFRESH()
		      ENDIF
		      SELECT wells
		   ENDIF
		ENDIF
		
		SET DELETED ON
		
		RETURN DODEFAULT()
		
		
		
	ENDPROC

