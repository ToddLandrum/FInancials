	PROCEDURE Pgfpageframe1.Page1.cboStatus.Valid
		LOCAL llFound
		
		IF INLIST(THIS.VALUE,'I','V','S','P') AND INLIST(wells.cWellStat,'A','N','T','D')  &&  If changing it from active to some other status, check the allocation files and new run, and warn them if there's items in there
		
		   SELECT expsusp
		   LOCATE FOR cwellid == wells.cwellid AND NOT DELETED()
		   IF FOUND()
		      llFound = .T.
		   ENDIF
		   SELECT incsusp
		   LOCATE FOR cwellid == wells.cwellid AND NOT DELETED()
		   IF FOUND()
		      llFound = .T.
		   ENDIF
		   IF llFound = .T.
		      THISFORM.oMessage.Warning("You are changing this well from being an active well, but there is income or expenses in the allocation file which won't get processed unless the well is active.")
		   ENDIF
		
		   llFound = .F.
		   SELECT income
		   LOCATE FOR cwellid == wells.cwellid AND nrunno = 0
		   IF FOUND()
		      llFound = .T.
		   ENDIF
		   SELECT expense
		   LOCATE FOR cwellid == wells.cwellid AND nrunnorev = 0 AND nrunnojib = 0 AND cyear <> 'FIXD'
		   IF FOUND()
		      llFound = .T.
		   ENDIF
		   IF llFound = .T.
		      THISFORM.oMessage.Warning("You are changing this well from being an active well, but there is income or expenses in the New Run which won't get processed unless the well is active.")
		   ENDIF
		ENDIF
		
		* Warn the user that chaning a well to a status of "Not Used" is irreversible
		IF THIS.VALUE = 'U'
		   IF NOT THISFORM.oMessage.CONFIRM('Changing the well status to Not Used locks this well. It cannot be changed back to active. Do you want to continue?')
		      this.refresh()
		      RETURN .F.
		   ENDIF
		ENDIF
		
		* Can't change the well status of Not Used to anything else.
		IF THIS.VALUE <> 'U' AND wells.cWellStat = 'U'
		   THISFORM.oMessage.severe('A well marked as Not Used cannot be changed to any other status.')
		   this.refresh()
		   RETURN .F.
		ENDIF
		
		DODEFAULT()
		
	ENDPROC

