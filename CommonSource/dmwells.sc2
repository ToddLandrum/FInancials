	PROCEDURE Pgfpageframe1.Page5.chkLusesev.Click
		
		IF wells.ltaxexempt1 AND this.value
		   IF thisform.omessage.confirm('This well is marked exempt from tax 1. These rates override the tax 1 calculations ' + ;
		                                'for oil & gas. Do you want to do this and mark the well as non exempt from tax 1?')
		      thisform.cmdatamanager.replace('Wells','Ltaxexempt1',.f.)
		      thisform.pgfpageframe1.page5.chkTaxexempt1.Refresh()
		   ELSE
		      this.Value = .f.
		   ENDIF 
		ENDIF 
		
		IF THIS.VALUE AND NOT EMPTY(wells.cTable)  &&  Already a tax table specified, so warn them, and don't let them choose it
		   messagebox('There is a tax table assigned to this well. This option ' + ;
		              'will replace the tax table tax 1 calculation for oil & gas. ' + ;
		              'This option should normally only be used for calculating ' + ;
		              'Montana tribal taxes.',48,'Important tax calculation information')
		ENDIF
		
		IF this.Parent.chkroysevtx.value
		   MESSAGEBOX('This option cannot be enabled when the option to exempt royalty owners from taxes is enabled.',16,'Conflicting Option')
		   this.Value = .f.
		ENDIF 
		
		IF THIS.VALUE
		   THIS.PARENT.txtnroysevo.ENABLED = .T.
		   THIS.PARENT.txtnroysevg.ENABLED = .T.
		   THIS.PARENT.txtnwrksevo.ENABLED = .T.
		   THIS.PARENT.txtnwrksevg.ENABLED = .T.
		ELSE
		   THIS.PARENT.txtnroysevo.ENABLED = .F.
		   THIS.PARENT.txtnroysevg.ENABLED = .F.
		   THIS.PARENT.txtnwrksevo.ENABLED = .F.
		   THIS.PARENT.txtnwrksevg.ENABLED = .F.
		ENDIF
		
		
		
	ENDPROC

