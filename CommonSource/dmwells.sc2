	PROCEDURE well_on_hold		&& Checks to see if all interests in a well are on hold
		*
		* Checks the wellinv table to see if all owners are marked on hold
		*
		LOCAL lnHolds
		lnHolds = 0
		
		swselect('wellinv')
		SCAN FOR cwellid == wells.cwellid AND NOT lonHold 
		   swselect('investor')
		   SET ORDER to cownerid
		   IF SEEK(wellinv.cownerid) AND NOT lIntegGL
		      lnHolds = lnHolds + 1
		   ENDIF 
		ENDSCAN 
		
		swselect('wellinv')
		COUNT FOR cwellid == wells.cwellid TO lnInts
		
		IF lnHolds = 0 AND lnInts > 0
		   llHeld = .T.
		ELSE
		   llHeld = .F.
		ENDIF
		
		IF llHeld
		   THISFORM.pgfpageframe1.page4.cmdHold.CAPTION = 'Release Well'
		ELSE
		   THISFORM.pgfpageframe1.page4.cmdHold.CAPTION = 'Suspend Well'
		ENDIF
		
		
		
		
	ENDPROC

	PROCEDURE cmdatamanager.afternav
		LPARAMETERS cworkarea
		
		THISFORM.chkrefresh()
		THISFORM.purchaserdesc()
		
		
		IF m.cWorkArea = 'Wells'
		   lcWellID = wells.cwellid
		   =REQUERY('well_owners')
		   THISFORM.pgfpageframe1.page4.cboOwnerID.clear()
		   THISFORM.pgfpageframe1.page4.cboOwnerID.REQUERY()
		   THISFORM.pgfpageframe1.page4.cboOwnerID.REfresh()
		   IF NOT wells.lsev1o
		      thisform.pgfpageframe1.page5.check1.Set('Enabled',.t.)
		   ELSE
		      thisform.pgfpageframe1.page5.check1.Set('Enabled',.f.)
		   ENDIF 
		   IF NOT wells.lsev1g       
		      thisform.pgfpageframe1.page5.Check2.Set('Enabled',.t.)
		   ELSE
		      thisform.pgfpageframe1.page5.Check2.Set('Enabled',.f.)
		   ENDIF       
		ENDIF 
		
		SELE land
		LOCATE FOR cleaseid = wells.cleaseid
		IF FOUND()
		   THISFORM.pgfpageframe1.page1.txtleasename.VALUE = land.cleasename
		ELSE
		   THISFORM.pgfpageframe1.page1.txtleasename.VALUE = ''
		ENDIF
		
		IF wells.lusesev = .T.  &&  Marked to use these rates insted of severance tax table, so enable the rate boxes
		   THISFORM.pgfpageframe1.Page5.txtNroysevo.SET('enabled',.T.)
		   THISFORM.pgfpageframe1.Page5.txtnroysevg.SET('enabled',.T.)
		   THISFORM.pgfpageframe1.Page5.txtnwrksevo.SET('enabled',.T.)
		   THISFORM.pgfpageframe1.Page5.txtnwrksevg.SET('enabled',.T.)
		ELSE
		   THISFORM.pgfpageframe1.Page5.txtNroysevo.SET('enabled',.F.)
		   THISFORM.pgfpageframe1.Page5.txtnroysevg.SET('enabled',.F.)
		   THISFORM.pgfpageframe1.Page5.txtnwrksevo.SET('enabled',.F.)
		   THISFORM.pgfpageframe1.Page5.txtnwrksevg.SET('enabled',.F.)
		ENDIF
		
		IF m.cworkarea = 'Wells'
		   THISFORM.coldgroup = wells.cgroup
		ENDIF
		
		thisform.well_on_hold()
		
		RETURN .t. 
		
	ENDPROC

	PROCEDURE cmdatamanager.afternew
		LPARAMETERS cworkarea
		LOCAL lcgroup
		lcgroup = '00'
		
		SELECT groups  &&  Don't fill in 00 as the group when adding a new record if they've been dumb, and deleted it.  Instead, just use the first group in the list.
		LOCATE FOR cGroup = '00' AND NOT DELETED()
		IF NOT FOUND()
		   SET ORDER TO cGroup
		   GO TOP
		   lcgroup = cGroup
		ENDIF
		SELECT wells
		
		WITH THISFORM.PGFPAGEFRAME1
