	PROCEDURE cmdatamanager.beforedelete
		LPARAMETERS cworkarea
		LOCAL lcWellID, lnCount
		
		if not used('expense')
		   use expense in 0
		endif   
		SELECT expense
		LOCATE FOR cWellID == wells.cwellid
		IF FOUND()
		   THISFORM.oMessage.Warning('There have been expenses entered using this well.  It cannot be deleted.')
		   RETURN .F.
		ENDIF
		
		if not used('income')
		   use income in 0
		endif   
		SELECT income
		LOCATE FOR cWellID == wells.cwellid
		IF FOUND()
		   THISFORM.oMessage.Warning('There have been income entered using this well.  It cannot be deleted.')
		   RETURN .F.
		ENDIF
		
		if not used('glmaster')
		   use glmaster in 0
		endif   
		SELECT glmaster
		LOCATE FOR cUnitNo == wells.cwellid
		IF FOUND()
		   THISFORM.oMessage.Warning('There have been journal entries posted using this well.  It cannot be deleted.')
		   RETURN .F.
		ENDIF
		
		if not used('disbhist')
		   use disbhist in 0
		endif   
		SELECT disbhist
		LOCATE FOR cwellid == wells.cwellid
		IF FOUND()
		   THISFORM.oMessage.Warning('There have been history entries made using this well.  It cannot be deleted.')
		   RETURN .F.
		ENDIF
		
		if not used('suspense')
		   use suspense in 0
		endif   
		SELECT suspense
		LOCATE FOR cwellid == wells.cwellid
		IF FOUND()
		   THISFORM.oMessage.Warning('There have been suspense entries made using this well.  It cannot be deleted.')
		   RETURN .F.
		ENDIF
		
		lcWellId = wells.cwellid
		=REQUERY('well_owners')
		SELECT well_owners
		COUNT FOR NOT DELETED() TO lnCount
		IF lnCount <> 0
		   THISFORM.oMessage.Warning('There has been division of interests created for this well.  It cannot be deleted.')
		   RETURN .F.
		ENDIF
		
		RETURN dodefault(cworkarea)
		
		
	ENDPROC

	PROCEDURE cmdatamanager.beforenew
		LPARAMETERS cworkarea
		
		IF m.goApp.lQBVersion
		   IF m.goApp.capplevel <> 'Unlimited'
		      lnwells = 0
		      IF NOT USED('wells1')
		         USE wells IN 0 AGAIN ALIAS wells1
		      ENDIF
		      SELECT wells1
		      COUNT FOR NOT DELETED() AND cwellstat <> 'U' TO lnwells
		      IF m.goApp.capplevel = 'Basic' AND lnwells >= 20
		         MESSAGEBOX('This is a basic version. 20 wells is the maximum number of wells allowed. ' + CHR(10) + ;
		            'You cannot add additional wells until you upgrade to the next level.',0,'Basic Version')
		         RETURN .F.
		      ENDIF
		   ENDIF
		ENDIF
		
	ENDPROC

	PROCEDURE cmdatamanager.beforeupdate
		LPARAMETERS cworkarealist
		
		IF m.goapp.lQBVersion
		   SELECT wells1
		   COUNT FOR NOT DELETED() AND cwellstat <> 'U' TO lnwells
		   IF m.goApp.capplevel = 'Basic' AND lnwells > 20
		      IF wells.cwellstat <> 'U'
		         MESSAGEBOX('This is a basic version. 20 wells is the maximum number of wells allowed. ' + CHR(10) + ;
		         'You cannot add additional wells until you upgrade to the next level.',0,'Basic Version')
		         RETURN .F.
		      ENDIF 
		   ENDIF
		ENDIF    
		
		SELE wells
		REPL dChanged WITH DATETIME(), ;
		   cChgUser WITH m.goApp.cuser
		
		IF THISFORM.coldgroup <> wells.cgroup
		   IF NOT USED('suspense')
		      USE suspense IN 0
		   ENDIF
		   SELECT suspense
		   COUNT FOR cWellID == wells.cWellID AND cgroup == THISFORM.coldgroup TO lnx
		   IF lnx > 0
		      IF THISFORM.omessage.CONFIRM('This well has suspense entries associated with group: ' + THISFORM.coldgroup + ;
		            ' Changing the group will affect the suspense balances reported when past owner statements are reprinted.' + ;
		            ' Do you want to continue and change those entries to this new group: ' + wells.cgroup+'?')
		         SELECT suspense
		         SCAN FOR suspense.cWellID == wells.cWellID AND suspense.cgroup == THISFORM.coldgroup
		            REPLACE cgroup WITH wells.cgroup
		         ENDSCAN
		      ELSE
		         THISFORM.omessage.warning('The changes were not saved.')
		         RETURN .F.
		      ENDIF
		   ENDIF
		ENDIF
		
		THISFORM.coldgroup = wells.cgroup  &&  When saved, set the cOldGroup variable, so if they change the group for a well multiple times without changing recs, it will work
		
		RETURN .T.
		
	ENDPROC

