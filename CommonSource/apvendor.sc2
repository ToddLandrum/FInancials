		   IF ' ' $ ALLTRIM(THIS.VALUE)
		      IF THISFORM.omessage.CONFIRM('Do you want to reverse this name for the sort field?')
		         lnx = RAT(' ',ALLTRIM(THIS.VALUE))
		
		         lcLast  = ALLTRIM(SUBSTR(THIS.VALUE,lnX))
		         lcFirst = LEFT(THIS.VALUE,lnX-1)
		
		         m.cSortField = lcLast + ', ' + lcFirst
		         THISFORM.cmdatamanager.REPLACE('Vendor','Csortfield',m.cSortField)
		         THISFORM.lchanged = .T.
		         THISFORM.PAGEFRAME.pagaddress.txtSortField.REFRESH()
		      ELSE
		         THISFORM.cmdatamanager.REPLACE('Vendor','Csortfield',THIS.VALUE)
		         THISFORM.lchanged = .T.
		         THISFORM.PAGEFRAME.pagaddress.txtSortField.REFRESH()
		      ENDIF
		   ELSE
		      THISFORM.cmdatamanager.REPLACE('Vendor','Csortfield',THIS.VALUE)
		      THISFORM.lchanged = .T.
