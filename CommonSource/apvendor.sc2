		thisform.csavevendor = this.Value
		DODEFAULT()
	ENDPROC

	PROCEDURE txtCvendorid.Valid
		lcVendorID = THIS.VALUE
		IF NOT EMPTY(lcVendorID) AND lcVendorID <> vendor.cvendorid
		   SELECT vendor1
		   SET ORDER TO cvendorid
		   SET DELE OFF
		   IF SEEK(lcVendorID)
		      IF NOT DELETED()
		         IF THISFORM.omessage.CONFIRM('There is already a vendor with this ID.  Would you like to edit the existing record?')
		            THISFORM.cmdatamanager.revertall()
		            SELECT vendor
		            SET ORDER TO cvendorid
		            LOCATE FOR cvendorid = lcVendorID
		            THISFORM.REFRESH()
		         ELSE
		            THIS.VALUE = ''
		            RETURN 0
		         ENDIF
		      ELSE
		         THISFORM.cmdatamanager.revertall()
		         SELECT vendor
		         SET ORDER TO cvendorid
		         SEEK(lcVendorID)
		         RECALL
		         SCATTER MEMVAR BLANK
		         GATHER MEMVAR
		         REPLACE vendor.cvendorid WITH lcVendorID
		         THISFORM.REFRESH()
		      ENDIF
		   ENDIF
		   SET DELE ON
		   SELECT vendor
		ENDIF
		
		DODEFAULT()
		
	ENDPROC

