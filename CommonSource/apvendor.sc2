	PROCEDURE Init
		DODEFAULT()
		
		IF TYPE('m.goApp') = 'O'
		   IF m.goApp.lAMVersion
		      thisform.pageframe.pagaddress.chkskipcheck.visible = .f.
		   ENDIF
		ELSE   
		   thisform.pageframe.pagaddress.chkskipcheck.visible = .f.
		ENDIF      
		
		IF m.goapp.lDocuments
		   thisform.AddObject('oFileDialog','sffiledialog')
		   thisform.pageFrame.PageCount = 6
		ELSE
		   thisform.pageFrame.PageCount = 5
		ENDIF    
		 
		
	ENDPROC

	PROCEDURE list
		LPARAMETERS cWorkarea
		*
		PRIV llOK
		LOCAL lcList, lnRecNo, lStatus
		* Default to the form's primary workarea
		IF EMPTY(m.cWorkarea)
		   m.cWorkarea = THIS.cWorkarea
		ENDIF
		
		IF THIS.flushcontrolbuffer() AND THIS.beforenav('Vendor')
		   THIS.RefreshAll()  && In case BeforeNav() did a Revert()
		
		   llOK = .T.
		
		   lcList = 'cvendorid,csortfield'
		
		   SELECT vendor
		   lnRecNo = RECNO()
		   COUNT FOR NOT DELETED() TO lnCount
		   IF lnCount = 0
		      RETURN
		   ENDIF
		
		   DO FORM ..\CUSTOM\picklist WITH 'Vendor', lcList, THISFORM.txtcvendorid.VALUE, 2
		
		   IF NOT llOK
		      GOTO lnRecNo
		   ENDIF
		
		   THISFORM.RefreshAll()
		ELSE
		   RETURN
		ENDIF
		
	ENDPROC

	PROCEDURE viewdocument
		LOCAL lnReturn, llReturn, lcOutputFile
		LOCAL liKey, lcPath, llDebug
		
		llDebug = FILE('datafiles\debug.txt')
		
		liKey = INT(VAL(thisform.pageframe.page6.lstDocuments.value))
		
		swselect('documents')
		SET ORDER TO iDocPK
		IF SEEK(liKey)
		   lcOutputFile = ALLTRIM(cPath)
		
		   llReturn = .T.
		
		   IF NOT FILE(lcOutputFile)
		      llReturn = .F.
		      IF llDebug
		         MESSAGEBOX('Attempting to view: ' + CHR(10) + ;
		                    lcOutputfile,64,'View Document')
		      ENDIF               
		   ENDIF
		
		   TRY
		      IF llReturn
		         DECLARE INTEGER ShellExecute IN "Shell32.dll" ;
		            INTEGER HWND, ;
		            STRING lpVerb, ;
		            STRING lpFile, ;
		            STRING lpParameters, ;
		            STRING lpDirectory, ;
		            LONG nShowCmd
		
		         lnReturn = ShellExecute(0, "Open", lcOutputFile, "", "", 1)
		         
		      ENDIF
		
		*      THIS.nReturnValue = lnReturn
		
		      IF lnReturn > 32
		         llReturn = .T.
		      ELSE
		         llReturn = .F.
		         IF lldebug
		            MESSAGEBOX('Return from ShellExecute: ' + TRANSFORM(lnReturn),64,'View Document')
		         ENDIF    
		      ENDIF
		   CATCH TO loError
		      IF llDebug
		         MESSAGEBOX('Error Occured: ' + CHR(10) + ;
		                     loError.Message,64,'View Document')
		      ENDIF                
		   ENDTRY
		
		   RETURN (llReturn)
		
		ENDIF
	ENDPROC

	PROCEDURE cmdatamanager.afternav
		LPARAMETERS cworkarea
		DODEFAULT(cworkarea)
		
		IF THISFORM.PAGEFRAME.ACTIVEPAGE = 4  &&  If history tab is active, calculate history info
		   THISFORM.PAGEFRAME.Page4.ACTIVATE()
		ENDIF
		
		IF THISFORM.PAGEFRAME.ACTIVEPAGE = 3  &&  If misc tab is active, disable insurance date if insurance option is not checked
		   if not vendor.lInsurance
		      ThisForm.PageFrame.pagCodes.txtDinsdate.enabled = .f.
		   else
		      ThisForm.PageFrame.pagCodes.txtDinsdate.enabled = .t.
		   ENDIF
		   IF NOT vendor.lvendcomp
		      thisform.pageFrame.pagcodes.txtdvendComp.Enabled = .f.
		   ELSE
		      thisform.pageFrame.pagcodes.txtdvendComp.Enabled = .t.
		   ENDIF 
		ENDIF
		
		IF m.goapp.lDocuments
		   thisform.pageFrame.page6.lstDocuments.Requery()
		ENDIF 
		
		thisform.csavevendor = vendor.cvendorid
		
	ENDPROC

	PROCEDURE cmdatamanager.afternew
		LPARAMETERS cworkarea
		
		IF m.cworkarea = 'Vendor'
		   REPLACE lSend1099 WITH .T., ;
		           itaxidtype WITH 1, ;
		           dadded WITH DATE() ;
		           caddedby WITH m.goapp.cuser
		ENDIF 
		
		thisform.setnextcontrol(thisform.txtcVendorID)
		
		return(DODEFAULT(cWorkarea))
	ENDPROC

	PROCEDURE cmdatamanager.beforedelete
		LPARAMETERS cworkarea
		
		SELECT appurchh
		LOCATE FOR cvendorid = vendor.cvendorid AND NOT DELETED()
		IF FOUND()
		   THISFORM.oMessage.Warning('You cannot delete this vendor because bills have been entered for them.')
		   RETURN .F.
		ENDIF
		
		SELECT expense
		LOCATE FOR cVendorID = vendor.cVendorID AND NOT DELETED()
		IF FOUND()
		   THISFORM.oMessage.Warning('You cannot delete this vendor because well expenses have been entered for them.')
		   RETURN .F.
		ENDIF
		
		IF m.goapp.lAMVersion
		   swselect('csdishdr')
		   LOCATE FOR cID = vendor.cVendorID AND NOT DELETED()
		   IF FOUND()
		      THISFORM.oMessage.Warning('You cannot delete this vendor because a cash disbursement has been entered for them.')
		      RETURN .F.
		   ENDIF
		ENDIF
		
		DODEFAULT(cworkarea)
		
	ENDPROC

	PROCEDURE cmdatamanager.beforenew
		LPARAMETERS cworkarea
		
		thisform.pageframe.activepage = 1
		thisform.setnextcontrol(thisform.txtcVendorID)
		
		return(dodefault(cworkarea))
	ENDPROC

	PROCEDURE cmdatamanager.beforeupdate
		LPARAMETERS cworkarealist
		
		IF EMPTY(vendor.cVendorID) OR EMPTY(vendor.cVendName)
		   thisform.oMessage.Warning('You cannot save a vendor with a blank vendor ID or vendor name.  Please correct before saving.')
		   RETURN .f.
		ENDIF
		
		IF NOT EMPTY(vendor.cVendorID)
		   IF THISFORM.cmdatamanager.isnew('Vendor')  &&  Only check this on new records, so it doesn't complain on updates to existing ones
		      SELECT investor
		      LOCATE FOR cOwnerID == vendor.cVendorID
		      IF FOUND()  &&  Don't let them have duplicate IDs
		         THISFORM.oMessage.Warning('There is already an owner defined with this same ID.  To avoid reporting errors, you cannot use the same ID for a vendor.')
		         THISFORM.setnextcontrol(THISFORM.txtCvendorid)
		         RETURN .F.
		      ENDIF
		
		      IF m.goapp.lAmversion
		         SWSELECT('custs')
		         LOCATE FOR cCustID == vendor.cVendorID
		         IF FOUND()  &&  Don't let them have duplicate IDs
		            THISFORM.oMessage.Warning('There is already a customer defined with this same ID.  To avoid reporting errors, you cannot use the same ID for a vendor.')
		            THISFORM.setnextcontrol(THISFORM.txtCvendorid)
		            RETURN .F.
		         ENDIF
		      ENDIF
		   ENDIF
		ENDIF
		
		IF EMPTY(vendor.cbaddr1) AND NOT EMPTY(vendor.cAddress1)
		   IF THISFORM.oMessage.CONFIRM('Should the mailing information be copied to the billing information?')
		      THISFORM.cmdatamanager.REPLACE('Vendor','Cbaddr1',vendor.cAddress1)
		      THISFORM.cmdatamanager.REPLACE('Vendor','Cbaddr2',vendor.caddress2)
		      THISFORM.cmdatamanager.REPLACE('Vendor','Cbcity',vendor.ccity)
		      THISFORM.cmdatamanager.REPLACE('Vendor','Cbstate',vendor.cstate)
		      THISFORM.cmdatamanager.REPLACE('Vendor','Cbzip',vendor.czip)
		      THISFORM.cmdatamanager.REPLACE('Vendor','cbphone',vendor.cphone)
		      THISFORM.cmdatamanager.REPLACE('Vendor','cbfaxphone',vendor.cfaxphone)
		      THISFORM.cmdatamanager.REPLACE('Vendor','cbContact',vendor.cContact)
		   ENDIF
		ENDIF
		
		IF EMPTY(vendor.cbaddr1) AND NOT EMPTY(vendor.cAddress1)
		   IF THISFORM.oMessage.CONFIRM('Should the mailing information be copied to the billing information?')
		      THISFORM.cmdatamanager.REPLACE('Vendor','Cbaddr1',vendor.cAddress1)
		      THISFORM.cmdatamanager.REPLACE('Vendor','Cbaddr2',vendor.caddress2)
		      THISFORM.cmdatamanager.REPLACE('Vendor','Cbcity',vendor.ccity)
		      THISFORM.cmdatamanager.REPLACE('Vendor','Cbstate',vendor.cstate)
		      THISFORM.cmdatamanager.REPLACE('Vendor','Cbzip',vendor.czip)
		      THISFORM.cmdatamanager.REPLACE('Vendor','cbphone',vendor.cphone)
		      THISFORM.cmdatamanager.REPLACE('Vendor','cbfaxphone',vendor.cfaxphone)
		      THISFORM.cmdatamanager.REPLACE('Vendor','cbContact',vendor.cContact)
		   ENDIF
		ENDIF
		
		IF NOT this.isnew('Vendor')
		   thisform.cmdatamanager.replace('Vendor','dchanged',DATE())
		   thisform.cmdatamanager.replace('Vendor','cchangedby',m.goapp.cuser)
		ENDIF    
		
		SELECT vendor1
		LOCATE FOR ALLTRIM(UPPER(vendor.cVendName)) $ UPPER(cVendName) AND cVendorID <> THISFORM.csavevendor AND THISFORM.csavevendor <> ' '  &&  Check for duplicate name, and give them a warning about possibly screwing up reports
		IF FOUND()
		   THISFORM.oMessage.Warning('The name you have assigned this vendor already exists as part of another vendor name.  Be aware that duplicate names can cause reporting errors when reports are sorted by name.')
		ENDIF
		
		DODEFAULT(cworkarealist)
		
	ENDPROC

