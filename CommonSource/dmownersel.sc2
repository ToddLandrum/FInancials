	PROCEDURE Init
		LPARA tcYear
		LOCAL ARRAY laAvail[1], laSelected[1]
		
		IF DODEFAULT()
		   STORE ' ' TO laAvail, laSelected
		
		   CREATE CURSOR usersel ;
		      (cownerID   C(10))
		   INDEX ON cownerID TAG cownerID
		
		   WAIT WINDOW NOWAIT 'Building List of owners...please wait'
		   SET TALK ON 
		   *
		   *  Load the available array with the available owners
		   *
		   SELECT cownerID + ' - ' + cownName ;
		         FROM investor ;
		         INTO ARRAY laAvail ;
		         ORDER BY cownerID
		   SET TALK off
		      THISFORM.lbl1099Total.CAPTION    = ALLTRIM(STR(_TALLY)) + ' Total Owners'
		      THISFORM.lbl1099Selected.CAPTION = '0 Owners Selected'
		
		   THISFORM.cntmoverlistpair.setvalue(@laAvail,@laSelected)
		
		ENDIF
		
		
		
		
		
	ENDPROC

	PROCEDURE cmdCancel.Click
		thisform.release()
	ENDPROC

	PROCEDURE cmdOK.Click
		LOCAL ARRAY laSelected[1]
		LOCAL lnCount, lnX
		
		laSelected = ' '
		
		thisform.cntmoverlistpair.getvalue(@laSelected)
		
		lnCount = ALEN(laSelected,1)
		
		FOR lnX = 1 TO lnCount  
		  m.cOwnerID = laSelected[lnX]
		  IF NOT EMPTY(m.cOwnerID)
		     INSERT INTO usersel FROM MEMVAR
		  ENDIF   
		ENDFOR
		
		thisform.release()  
		 
	ENDPROC

	PROCEDURE Cntmoverlistpair.deselectitem
		LPARAMETERS nindex
		
		DODEFAULT(nindex)
		
		thisform.nSelected = thisform.nSelected - 1
		
		IF thisform.nSelected < 0
		   thisform.nSelected = 0
		ENDIF   
		
		thisform.lbl1099Selected.Caption = ALLTRIM(STR(thisform.nSelected)) + ' Owners Selected'
		
	ENDPROC

	PROCEDURE Cntmoverlistpair.selectitem
		LPARAMETERS nindex
		
		DODEFAULT(nindex)
		
		thisform.nSelected = thisform.nSelected + 1
		
		thisform.lbl1099Selected.Caption = ALLTRIM(STR(thisform.nSelected)) + ' Owners Selected'
		
		
	ENDPROC

