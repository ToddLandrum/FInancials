	PROCEDURE cmdatamanager.beforenew
		LPARAMETERS cworkarea
		
		IF m.goApp.lQBVersion
		   IF m.goApp.capplevel <> 'Unlimited'
		      lnwells = 0
		      IF NOT USED('wells1')
		         USE wells IN 0 AGAIN ALIAS wells1
		      ENDIF
		      SELECT wells1
		      COUNT FOR NOT DELETED() AND cwellstat <> 'U' TO lnwells
		      IF m.goApp.capplevel = 'Basic' AND lnwells >= 20
		         MESSAGEBOX('This is a basic version. 20 wells is the maximum number of wells allowed. ' + CHR(10) + ;
		            'You cannot add additional wells until you upgrade to the next level.',0,'Basic Version')
		         RETURN .F.
		      ENDIF
		   ENDIF
		ENDIF
		
	ENDPROC

	PROCEDURE cmdatamanager.beforeupdate
		LPARAMETERS cworkarealist
		
		IF m.goapp.lQBVersion
		   SELECT wells1
		   COUNT FOR NOT DELETED() AND cwellstat <> 'U' TO lnwells
		   IF m.goApp.capplevel = 'Basic' AND lnwells > 20
		      IF wells.cwellstat <> 'U'
		         MESSAGEBOX('This is a basic version. 20 wells is the maximum number of wells allowed. ' + CHR(10) + ;
		         'You cannot add additional wells until you upgrade to the next level.',0,'Basic Version')
		         RETURN .F.
		      ENDIF 
		   ENDIF
		ENDIF    
		
		SELE wells
		REPL dChanged WITH DATETIME(), ;
		   cChangedBy WITH m.goApp.cuser
		
		IF THISFORM.coldgroup <> wells.cgroup
		   IF NOT USED('suspense')
		      USE suspense IN 0
		   ENDIF
		   SELECT suspense
		   COUNT FOR cWellID == wells.cWellID AND cgroup == THISFORM.coldgroup TO lnx
		   IF lnx > 0
		      IF THISFORM.omessage.CONFIRM('This well has suspense entries associated with group: ' + THISFORM.coldgroup + ;
		            ' Changing the group will affect the suspense balances reported when past owner statements are reprinted.' + ;
		            ' Do you want to continue and change those entries to this new group: ' + wells.cgroup+'?')
		         SELECT suspense
		         SCAN FOR suspense.cWellID == wells.cWellID AND suspense.cgroup == THISFORM.coldgroup
		            REPLACE cgroup WITH wells.cgroup
		         ENDSCAN
		      ELSE
		         THISFORM.omessage.warning('The changes were not saved.')
		         RETURN .F.
		      ENDIF
		   ENDIF
		ENDIF
		
		THISFORM.coldgroup = wells.cgroup  &&  When saved, set the cOldGroup variable, so if they change the group for a well multiple times without changing recs, it will work
		
		RETURN .T.
		
	ENDPROC

	PROCEDURE cmdHold.Click
		swselect('investor')
		SET ORDER TO cownerid
		swselect('wellinv',.T.)
		IF THIS.CAPTION = 'Suspend Well'
		   IF THISFORM.omessage.CONFIRM('This will mark all owner interests as held in the division of interests for this well. Continue?')
		      SELECT wellinv
		      SCAN FOR cwellid == wells.cwellid
		         SELECT investor
		         IF SEEK(wellinv.cownerid) AND linteggl
		            LOOP
		         ENDIF
		         SELECT wellinv
		         REPLACE lonhold WITH .T.
		      ENDSCAN
		      TABLEUPDATE(.T.)
		      MESSAGEBOX('All interests in this well have been placed on hold. ' + CHR(10) + CHR(10) +;
		         'Changing the division of interests manually by taking ' + CHR(10) + ;
		         'an interest off hold will change this button to say ' + CHR(10) + ;
		         '"Suspend Well" again.',64,'Release Well')
		   ENDIF
		ELSE
		   IF THISFORM.omessage.CONFIRM('This will mark all owner interests as not held in the division of interests for this well. Continue?')
		      SELECT wellinv 
		      SCAN FOR cwellid == wells.cwellid
		         REPLACE lonhold WITH .F.
		      ENDSCAN
		      TABLEUPDATE(.T.)
		      MESSAGEBOX('All interests in this well have been taken off hold. ' + CHR(10) + CHR(10) +;
		         'Changing the division of interests manually by placing ' + CHR(10) + ;
		         'an interest on hold will change this button to say ' + CHR(10) + ;
		         '"Release Well" again.',64,'Release Well')
		   ENDIF
		ENDIF
		
		THISFORM.well_on_hold()
		
	ENDPROC

	PROCEDURE cmdLease.Click
		LOCAL m.cWellID
		
		m.cWellID = thisform.txtwellid.Value 
		
		DO FORM dmsellease WITH m.cWellID
		
		SELECT leasesel
		COUNT FOR NOT DELETED() TO lnCount
		      
		IF lnCount > 0
		   SELECT cLeaseID, .F. as lExists FROM leasesel INTO CURSOR leaselink readwrite
		   
		   * Find out if lease is already attached to this well
		   SELECT leaselink
		   SCAN 
		      SCATTER MEMVAR 
		      SELECT leasewell
		      LOCATE FOR cwellid == m.cwellid AND cleaseid == m.cleaseid
		      IF FOUND()
		         SELECT leaselink
		         REPLACE lExists WITH .t.
		      ENDIF
		   ENDSCAN 
		
		   * Only add leases not already attached
		   SELECT leaselink
		   SCAN FOR NOT lExists
		      SCATTER MEMVAR 
		      INSERT INTO leasewell FROM memvar
		   ENDSCAN
		   
		   * Remove leases no longer attached to this well
		   SWSELECT('leasewell')
		   SCAN FOR cwellid == m.cwellid
		      m.cLeaseID = cLeaseID
		      SELECT leaselink
		      LOCATE FOR cLeaseID == m.cLeaseID
		      IF NOT FOUND()
		         SELECT leasewell
		         DELETE NEXT 1
		      ENDIF
		   ENDSCAN 
		   
		   thisform.lstleases.Clear()
		   
		   lnI = 1
		   SWSELECT('leasewell')
		   SCAN FOR cWellID == m.cWellID
		      m.cLeaseID = cLeaseID
		      SELECT land
		      SET ORDER to cLeaseID
		      IF SEEK(m.cLeaseID)
		         m.cLeaseName = cLeaseName
		         thisform.lstLeases.AddListItem(m.cLeaseID + ' - ' + m.cLeaseName,lnI,1)
		         lnI = lnI + 1
		      ENDIF
		   ENDSCAN 
		   thisform.setstate()
		ELSE
		   swselect('leasewell')
		   DELETE FOR cwellid == m.cWellID
		   thisform.setstate()      
		ENDIF 
	ENDPROC

