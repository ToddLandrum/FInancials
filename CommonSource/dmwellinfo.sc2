		         thisform.pgfpageframe1.page2.txtPermit5.lEnabled = .F.
		         thisform.pgfpageframe1.page2.txtPermit4.lEnabled = .F.
		         thisform.pgfpageframe1.page2.txtPermit3.lEnabled = .F.
		         thisform.pgfpageframe1.page2.txtPermit2.lEnabled = .F.
