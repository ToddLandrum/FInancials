	PROCEDURE cmdatamanager.beforedelete
		LPARAMETERS cworkarea
		LOCAL lcWellID, lnCount
		
		if not used('expense')
		   use expense in 0
		endif   
		SELECT expense
		LOCATE FOR cWellID == wells.cwellid
		IF FOUND()
		   THISFORM.oMessage.Warning('There have been expenses entered using this well.  It cannot be deleted.')
		   RETURN .F.
		ENDIF
		
		if not used('income')
		   use income in 0
		endif   
		SELECT income
		LOCATE FOR cWellID == wells.cwellid
		IF FOUND()
		   THISFORM.oMessage.Warning('There have been income entered using this well.  It cannot be deleted.')
		   RETURN .F.
		ENDIF
		
		if not used('glmaster')
		   use glmaster in 0
		endif   
		SELECT glmaster
		LOCATE FOR cUnitNo == wells.cwellid
		IF FOUND()
		   THISFORM.oMessage.Warning('There have been journal entries posted using this well.  It cannot be deleted.')
		   RETURN .F.
		ENDIF
		
		if not used('disbhist')
		   use disbhist in 0
		endif   
		SELECT disbhist
		LOCATE FOR cwellid == wells.cwellid
		IF FOUND()
		   THISFORM.oMessage.Warning('There have been history entries made using this well.  It cannot be deleted.')
		   RETURN .F.
		ENDIF
		
		if not used('suspense')
		   use suspense in 0
		endif   
		SELECT suspense
		LOCATE FOR cwellid == wells.cwellid
		IF FOUND()
		   THISFORM.oMessage.Warning('There have been suspense entries made using this well.  It cannot be deleted.')
		   RETURN .F.
		ENDIF
		
		lcWellId = wells.cwellid
		=REQUERY('well_owners')
		SELECT well_owners
		COUNT FOR NOT DELETED() TO lnCount
		IF lnCount <> 0
		   THISFORM.oMessage.Warning('There has been division of interests created for this well.  It cannot be deleted.')
		   RETURN .F.
		ENDIF
		
		RETURN dodefault(cworkarea)
		
		
	ENDPROC

