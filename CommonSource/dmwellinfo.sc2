	PROCEDURE Init
		LPARAMETERS lcWellID
		
		IF DODEFAULT()
		   IF FILE('datafiles\northwood.txt')
		      thisform.lnorthwood = .t.
		      thisform.pgfpageframe1.page2.lbluser3.Caption = 'Tank #'
		   ENDIF
		   
		   IF VARTYPE(lcWellID) = 'C'
		      swselect('wells')
		      SEEK(lcWellID)
		   ENDIF 
		   IF m.goapp.lDocuments
		      thisform.pgfpageframe1.page6.Enabled = .t.
		   ELSE
		      thisform.pgfpageframe1.page6.Enabled = .f.
		   ENDIF    
		   
		   thisform.loadleases()
		ENDIF
		
	ENDPROC

	PROCEDURE list
		LPARAMETERS cworkarea
		LOCAL lcList, lnRecNo, llDontMove
		PRIV llOK
		
		IF thisform.lnorthwood
		   thisform.listnorthwood()
		   RETURN
		ENDIF 
		
		llOK = .F.
		llDontMove = .F.
		
		lcList = 'cwellid,cwellname,cgroup,cwellstat'
		
		SELECT wells
		lnRecNo = RECNO()
		
		IF thisform.cmdatamanager.isnew('Wells')
		   llDontMove = .T.
		ENDIF 
		IF this.flushcontrolbuffer() AND this.beforenav('Wells')
