	PROCEDURE cboTable.Valid
		IF DODEFAULT()
		
		   *Don't let them have both a tax table, and the option about using the rates instead of a tax table marked
		   IF NOT EMPTY(this.Value) AND wells.lUseSev  
		      thisform.oMessage.Warning('You currently have the option marked on the Taxes tab to specify the rates instead of using a tax table.  In order to select a tax table, first un-mark that option.')
		      this.set('value','')      
		   ENDIF
		   
		   * Don't let them set this option if it's a 3rd party well
		   IF NOT EMPTY(this.value) AND wells.lThird_prty  
		      THISFORM.oMessage.Warning('You cannot specify a tax table for this well because it is marked as a 3rd party well, which means you are not liable for paying the taxes being withheld.')
		      this.set('value','')
		   ENDIF
		
		   IF NOT EMPTY(this.Value) AND ;
		      (wells.lsev1o OR wells.lsev2o OR wells.lsev3o OR wells.lsev4o OR ;
		       wells.lsev1g OR wells.lsev2g OR wells.lsev3g OR wells.lsev4g OR ;
		       wells.lsev1p OR wells.lsev2p OR wells.lsev3p OR wells.lsev4p)
		      
		      thisform.cmdatamanager.replace('Wells','Lsev1o',.F.)
		      thisform.cmdatamanager.replace('Wells','Lsev2o',.F.)
		      thisform.cmdatamanager.replace('Wells','Lsev3o',.F.)
		      thisform.cmdatamanager.replace('Wells','Lsev4o',.F.)
		      thisform.cmdatamanager.replace('Wells','Lsev1g',.F.)
		      thisform.cmdatamanager.replace('Wells','Lsev2g',.F.)
		      thisform.cmdatamanager.replace('Wells','Lsev3g',.F.)
		      thisform.cmdatamanager.replace('Wells','Lsev4g',.F.)
		      thisform.cmdatamanager.replace('Wells','Lsev1p',.F.)
		      thisform.cmdatamanager.replace('Wells','Lsev2p',.F.)
		      thisform.cmdatamanager.replace('Wells','Lsev3p',.F.)
		      thisform.cmdatamanager.replace('Wells','Lsev4p',.F.)
		   ENDIF    
		ENDIF       
	ENDPROC

