	PROCEDURE Activate
		IF DODEFAULT()
		   thisform.well_on_hold()
		   thisform.loadleases()
		   IF wells.cstate = 'WV'
		      thisform.pgfpageframe1.page7.Enabled = .t.
		   ELSE
		      thisform.pgfpageframe1.page7.Enabled = .f.
		   ENDIF 
		ENDIF    
	ENDPROC

	PROCEDURE beforeclose
		LPARAMETERS nclosereason
		
		IF THISFORM.lchanged AND NOT thisform.isnew('Wells')
		    IF EMPTY(THISFORM.txtWellid.VALUE) OR EMPTY(THISFORM.txtWellname.VALUE)
		        THISFORM.oMessage.Warning('You cannot save a well record with a blank name or ID.  Please correct before proceeding.')
		        RETURN .F.
		    ENDIF
		ENDIF
		
		DODEFAULT(nclosereason)
		
	ENDPROC

	PROCEDURE changeuserlabels
		IF DODEFAULT()
		   DO CASE
		      CASE wells.cstate = 'OK'
		         THISFORM.pgfpageframe1.page2.lbluser1.CAPTION = 'PUN No'
		         THISFORM.pgfpageframe1.page2.lbluser2.CAPTION = 'Prod Code'
		         THISFORM.pgfpageframe1.page2.lbluser3.CAPTION = 'Disp Code'
		         THISFORM.pgfpageframe1.page2.lbluser4.CAPTION = 'Oper Code'
		         THISFORM.pgfpageframe1.page2.lbluser5.CAPTION = 'Oper Name'
		         THISFORM.pgfpageframe1.page2.lbluser6.CAPTION = 'BTU Factor'
		      CASE wells.cstate = 'WY'
		         THISFORM.pgfpageframe1.page2.lbluser1.CAPTION = 'Rpt Group'
		         THISFORM.pgfpageframe1.page2.lbluser2.CAPTION = 'Rate Code'
		         THISFORM.pgfpageframe1.page2.lbluser3.CAPTION = 'User 3'
		         THISFORM.pgfpageframe1.page2.lbluser4.CAPTION = 'User 4'
		         THISFORM.pgfpageframe1.page2.lbluser5.CAPTION = 'User 5'
		         THISFORM.pgfpageframe1.page2.lbluser6.CAPTION = 'User 6'
		      CASE wells.cstate = 'LA'
		         THISFORM.pgfpageframe1.page2.lbluser1.CAPTION = 'Parish No'
		         THISFORM.pgfpageframe1.page2.lbluser2.CAPTION = 'Taxpayer Code'
		         THISFORM.pgfpageframe1.page2.lbluser3.CAPTION = 'Tax Rate Code'
		         THISFORM.pgfpageframe1.page2.lbluser4.CAPTION = 'Excl Code'
		         THISFORM.pgfpageframe1.page2.lbluser5.CAPTION = 'Excl Rate'
		         THISFORM.pgfpageframe1.page2.lbluser6.CAPTION = 'LUW Code'
		         THISFORM.pgfpageframe1.page2.lblPermit6.CAPTION = 'Serial #'
		      CASE wells.cstate = 'OH'
		         THISFORM.pgfpageframe1.page2.lbluser1.CAPTION = 'Storage Tanks'
		         THISFORM.pgfpageframe1.page2.lbluser2.CAPTION = 'Max Capacity'
		         THISFORM.pgfpageframe1.page2.lbluser3.CAPTION = 'User 3'
		         THISFORM.pgfpageframe1.page2.lbluser4.CAPTION = 'User 4'
		         THISFORM.pgfpageframe1.page2.lbluser5.CAPTION = 'User 5'
		         THISFORM.pgfpageframe1.page2.lbluser6.CAPTION = 'User 6'
		      CASE wells.cstate = 'TX'
