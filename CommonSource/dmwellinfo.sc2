	PROCEDURE well_on_hold		&& Checks to see if all interests in a well are on hold
		*
		* Checks the wellinv table to see if all owners are marked on hold
		*
		LOCAL lnHolds
		lnHolds = 0
		
		swselect('wellinv')
		SCAN FOR cwellid == wells.cwellid AND NOT lonHold 
		   swselect('investor')
		   SET ORDER to cownerid
		   IF SEEK(wellinv.cownerid) AND NOT lIntegGL
		      lnHolds = lnHolds + 1
		   ENDIF 
		ENDSCAN 
		
		swselect('wellinv')
		COUNT FOR cwellid == wells.cwellid TO lnInts
		
		IF lnHolds = 0 AND lnInts > 0
		   llHeld = .T.
		ELSE
		   llHeld = .F.
		ENDIF
		
		IF llHeld
		   THISFORM.cmdHold.CAPTION = 'Release Well'
		ELSE
		   THISFORM.cmdHold.CAPTION = 'Suspend Well'
		ENDIF
		
		
		
		
	ENDPROC

