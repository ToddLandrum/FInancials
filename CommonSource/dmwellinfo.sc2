	PROCEDURE loadleases
		LOCAL lcWellID, llAlarm
		
		llAlarm = .f.
		
		* Clear the list before starting
		THISFORM.lstleases.CLEAR()
		
		lcWellID = wells.cwellid
		lnI      = 1
		SWSELECT('leasewell')
		SCAN FOR cwellid == lcWellID
			m.cLeaseID = cLeaseID
			SELECT land
			SET ORDER TO cLeaseID
