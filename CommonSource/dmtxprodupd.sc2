	PROCEDURE cmdatamanager.afterdelete
		LPARAMETERS cworkarea
		
		RETURN .t.
	ENDPROC

	PROCEDURE cmdatamanager.afternew
		LPARAMETERS cWorkarea
		
		thisform.txtcYear.set('value',ALLTRIM(STR(YEAR(DATE()))))
		thisform.txtcPeriod.set('value',PADL(ALLTRIM(STR(MONTH(DATE()))),2,'0'))
		
		DODEFAULT(cWorkArea)
	ENDPROC

	PROCEDURE cmdLease.Click
		local lclist
		priv llok
		
		llok = .f.
		
		lclist = 'cleaseid,cleasename,cprospect'
		
		do form picklist with 'Land', lcList, thisform.txtcleaseid.value, 3
		
		if llok
		   thisform.txtcleaseid.set('value',land.cleaseid)
		   thisform.txtcleasename.set('value',land.cleasename)
		   thisform.txtcrrcdistno.set('value',land.crrcdistno)
		   thisform.txtcrrcleaseno.set('value',land.crrcleaseno)
		   thisform.txtccommingle.set('value',land.ccomingle)
		   thisform.setnextcontrol(thisform.txtcleaseid)
		   thisform.refresh()
		endif   
	ENDPROC

	PROCEDURE cmdLookField.Click
		LOCAL lcList
		PRIV llOK
		
		llOK = .F.
		lcList = 'cfieldid,cfieldname'
		
		DO FORM ..\custom\picklist WITH 'Fields', lcList, thisform.txtcFieldID.value, 2
		
		IF llOK
		   IF fields.cfieldid <> txrrcdata.cfieldid
		      THISFORM.cmdatamanager.REPLACE('txrrcdata','cfieldid', fields.cfieldid)
		      thisform.cmdatamanager.replace('txrrcdata','cfieldname',fields.cfieldname)
		      THISFORM.txtcFieldID.REFRESH()
		      thisform.txtcfieldname.refresh()
		      THISFORM.lchanged = .T.
		   ENDIF
		ENDIF
		
		THISFORM.txtcFieldID.SETFOCUS()
		
	ENDPROC

	PROCEDURE txtcFieldID.GotFocus
		ON KEY LABEL F2 _screen.activeform.pgfPageFrame1.page3.cmdLookField.Click()
	ENDPROC

	PROCEDURE txtcFieldID.InteractiveChange
		thisform.cmdatamanager.replace('txrrcdata','cfieldname',fields.cfieldname)
		thisform.txtcfieldname.refresh()
		dodefault()
	ENDPROC

	PROCEDURE txtcFieldID.LostFocus
		on key label F2 *
		DODEFAULT()
	ENDPROC

	PROCEDURE txtcFieldID.Valid
		IF NOT EMPTY(THIS.VALUE)
		   SELECT FIELDS
		   SET ORDER TO cfieldid
		   IF NOT SEEK(TRIM(THIS.VALUE))
		      thisform.omessage.warning('The field code entered is not valid.')
		      RETURN 0
		   ENDIF   
		ENDIF
		
		DODEFAULT()
		
	ENDPROC

	PROCEDURE txtcLeaseID.InteractiveChange
		IF DODEFAULT()
		   SELE land
		   LOCATE FOR cleaseid = THIS.VALUE
		   IF FOUND()
		      THISFORM.txtcleaseid.SET('value',land.cleaseid)
		      THISFORM.txtcleasename.SET('value',land.cleasename)
		      THISFORM.txtcrrcdistno.SET('value',land.crrcdistno)
		      THISFORM.txtcrrcleaseno.SET('value',land.crrcleaseno)
		      THISFORM.txtccommingle.SET('value',land.ccomingle)
		   ELSE
		      THISFORM.txtcleaseid.SET('value','')
		      THISFORM.txtcleasename.SET('value','')
		      THISFORM.txtcrrcdistno.SET('value','')
		      THISFORM.txtcrrcleaseno.SET('value','')
		      THISFORM.txtccommingle.SET('value','')
		   ENDIF
		ENDIF
		
	ENDPROC

