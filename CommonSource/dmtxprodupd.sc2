	PROCEDURE calcendvol
		lnEnd = val(thisform.txtnbegvol.value) + val(thisform.txtnproduced.value) - val(thisform.txtndispvolume.value)
		thisform.txtnendvol.set('value',lnEnd)
		thisform.txtnendvol.refresh()
	ENDPROC

	PROCEDURE list
		LPARAMETERS cworkarea
		
		local lclist
		priv llok
		
		llok = .f.
		
		lclist = 'cyear,cperiod,cleasename'
		
		do form picklist with 'Txrrcdata', lcList, 'P1', 3
		
		if llok
		   thisform.refresh()
		endif   
	ENDPROC

