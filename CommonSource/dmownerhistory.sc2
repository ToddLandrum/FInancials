	PROCEDURE list
		LPARAMETERS cworkarea
		
		LOCAL lcList, llReturn, loError
		PRIV llOK
		
		llOK     = .F.
		llReturn = .T.
		
		TRY
		
		    IF THIS.flushcontrolbuffer() AND THIS.beforenav('ownhist')
		
		        SELECT ownhist
		        LOCATE FOR lManual AND NOT DELETED()  &&  See if there are any manual records
		        IF NOT FOUND()
		            THISFORM.oMessage.DISPLAY('This table is empty, so no list can be shown.')
		            llReturn = .F.
		            EXIT
		        ENDIF
		
		        IF THISFORM.oMessage.CONFIRM('Should the lookup list include the owner name? Including the owner name will take longer to bring the list up.')
		            lcList = 'cownerid\Owner ID,csortfield\Owner Name,cwellid\Well ID,cwellname\Well Name,hyear\Year,hperiod\Prd,ctypeinv\Type,nNetCheck\Net Check'
		
					SELECT  ownhist.cownerid, ;
							investor.csortfield, ;
							ownhist.cwellid, ;
							wells.cwellname, ;
							ownhist.hyear, ;
							ownhist.hperiod, ;
							ownhist.ctypeinv, ;
							ownhist.nNetCheck,  ;
							ownhist.ciddisb_a ;
					   FROM ownhist, investor, wells ;
					   WHERE ownhist.cownerid = investor.cownerid ;
						   AND ownhist.cwellid = wells.cwellid ;
						   AND ownhist.lManual = .T. ;
					   INTO CURSOR tempohist READWRITE
		
		            SET SAFETY off
		            SELECT tempohist
		            INDEX ON cownerid + hyear + hperiod + cwellid + ctypeinv TAG cownerid
		            INDEX ON csortfield + cwellid + hyear + hperiod + ctypeinv TAG cownname
		            INDEX ON cwellid + hyear + hperiod + cownerid + ctypeinv TAG cwellid
		            INDEX ON cwellname + hyear + hperiod + cownerid + ctypeinv TAG cwellname
		            INDEX ON hyear + hperiod + cownerid + cwellid + ctypeinv TAG hyear
		            INDEX ON hyear + hperiod + cownerid + cwellid + ctypeinv TAG hperiod
		            INDEX ON nNetCheck TAG nNetCheck
		
		            DO FORM ..\CUSTOM\picklist WITH 'tempohist', lcList, THISFORM.txtcownerid.VALUE, 8, .T., .T.
		
		            IF llOK
		                SELECT ownhist
		                LOCATE FOR ciddisb_a = tempohist.ciddisb_a
		                IF FOUND()
		                    THISFORM.REFRESH()
		                    THISFORM.cmdatamanager.afternav('ownhist')
		                ELSE  &&  Should never get here
		                    WAIT WINDOW NOWAIT 'Cannot find history record'
		                ENDIF
		            ENDIF
		        ELSE
		            lcList = 'cownerid\Owner ID,cwellid\Well ID,hyear\Year,hperiod\Prd,ctypeinv\Type,nNetCheck\Net Check'
		
					SELECT  ownhist.cownerid, ;
							ownhist.cwellid, ;
							ownhist.hyear, ;
							ownhist.hperiod, ;
							ownhist.ctypeinv, ;
							ownhist.nNetCheck,  ;
							ownhist.ciddisb_a, ;
							.F. AS junk  ;
					   FROM ownhist  ;
					   WHERE ownhist.lManual = .T.  ;
					   INTO CURSOR tempohist READWRITE
		
		            SELECT tempohist
		            INDEX ON cownerid + hyear + hperiod + cwellid + ctypeinv TAG cownerid
		            INDEX ON cwellid + hyear + hperiod + cownerid + ctypeinv TAG cwellid
		            INDEX ON hyear + hperiod + cownerid + cwellid + ctypeinv TAG hyear
		            INDEX ON hyear + hperiod + cownerid + cwellid + ctypeinv TAG hperiod
		            INDEX ON nNetCheck TAG nNetCheck
		
		
		            DO FORM ..\CUSTOM\picklist WITH 'tempohist', lcList, THISFORM.txtcownerid.VALUE, 8, .T., .T.
		
		            IF llOK
		                SELECT ownhist
		                LOCATE FOR ciddisb_a = tempohist.ciddisb_a
		                IF FOUND()
		                    THISFORM.REFRESH()
		                    THISFORM.cmdatamanager.afternav('ownhist')
		                ELSE  &&  Should never get here
		                    WAIT WINDOW NOWAIT 'Cannot find history record'
		                ENDIF
		            ENDIF
		        ENDIF
		
		        FOR lnx = 1 TO THISFORM.runcombo1.LISTCOUNT
		            IF THISFORM.runcombo1.LISTITEM[lnx, 2] = ownhist.cRunYear AND ;
		                    INT(VAL(THISFORM.runcombo1.LISTITEM[lnx, 3])) = ownhist.nRunNo AND ;
		                    THISFORM.runcombo1.LISTITEM[lnx, 4] = ownhist.cgroup AND ;
		                    THISFORM.runcombo1.LISTITEM[lnx, 6] = ownhist.crectype
		                THISFORM.runcombo1.LISTITEMID = lnx
		                EXIT
		            ENDIF
		        ENDFOR
		    ENDIF
		
		CATCH TO loError
		    llReturn = .F.
		    DO errorlog WITH 'List', loError.LINENO, 'Owner Hisotry', loError.ERRORNO, loError.MESSAGE, '', loError
		    MESSAGEBOX('Unable to process the list at this time. Check the System Log found under Other Reports for more information.' + CHR(10) + CHR(10) + ;
		          'Contact SherWare Support for help at support@sherware.com', 16, 'Problem Encountered')
		ENDTRY
		
		RETURN llReturn
		
		
		
		
		
	ENDPROC

