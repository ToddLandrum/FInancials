	PROCEDURE calctot		&& Calculates the total from the data found in each field of the table.
		LOCAL lnAmount
		LOCAL llReturn, loError
		
		lnAmount = 0
		llReturn = .T.
		
		TRY
		    DO CASE
		        CASE ownhist.cRecType = 'J'
		            lnAmount = ownhist.nExpense + ownhist.nTotale1 + ;
		                ownhist.nTotale2 + ownhist.nTotale3 + ;
		                ownhist.nTotale4 + ownhist.nTotale5
		        CASE ownhist.cRecType = 'R'
		            DO CASE
		                CASE ownhist.cdirect = 'N'
		                    lnAmount = ownhist.noilrev + ownhist.ngasrev + ownhist.nothrev + ;
		                        ownhist.ntrprev + ownhist.nmiscrev1 + ;
		                        ownhist.nmiscrev2 + ownhist.nflatrate - ;
		                        ownhist.noiltax1 - ownhist.ngastax1  - ;
		                        ownhist.noiltax2 - ownhist.ngastax2  - ;
		                        ownhist.noiltax3 - ownhist.ngastax3  - ;
		                        ownhist.noiltax4 - ownhist.ngastax4  - ;
		                        ownhist.nothtax1 - ownhist.nothtax2  - ;
		                        ownhist.nothtax3 - ownhist.nothtax4  - ;
		                        ownhist.nBackWith - ownhist.nTaxWith -  ;
		                        (ownhist.nExpense + ownhist.nTotale1 + ;
		                          ownhist.nTotale2 + ownhist.nTotale3 + ;
		                          ownhist.nTotale4 + ownhist.nTotale5 + ;
		                          ownhist.nTotaleA + ownhist.nTotaleB + ;
		                          ownhist.ngather  + ownhist.ncompress)
		                CASE ownhist.cdirect = 'O'
		                    lnAmount = ownhist.ngasrev + ownhist.nothrev + ;
		                        ownhist.ntrprev + ownhist.nmiscrev1 + ;
		                        ownhist.nmiscrev2 + ownhist.nflatrate - ;
		                        ownhist.noiltax1 - ownhist.ngastax1  - ;
		                        ownhist.noiltax2 - ownhist.ngastax2  - ;
		                        ownhist.noiltax3 - ownhist.ngastax3  - ;
		                        ownhist.noiltax4 - ownhist.ngastax4  - ;
		                        ownhist.nothtax1 - ownhist.nothtax2  - ;
		                        ownhist.nothtax3 - ownhist.nothtax4  - ;
		                        ownhist.nBackWith - ownhist.nTaxWith -  ;
		                        (ownhist.nExpense + ownhist.nTotale1 + ;
		                          ownhist.nTotale2 + ownhist.nTotale3 + ;
		                          ownhist.nTotale4 + ownhist.nTotale5 + ;
		                          ownhist.nTotaleA + ownhist.nTotaleB + ;
		                          ownhist.ngather  + ownhist.ncompress)
		                CASE ownhist.cdirect = 'G'
		                    lnAmount = ownhist.noilrev + ownhist.nothrev + ;
		                        ownhist.ntrprev + ownhist.nmiscrev1 + ;
		                        ownhist.nmiscrev2 + ownhist.nflatrate - ;
		                        ownhist.noiltax1 - ownhist.ngastax1  - ;
		                        ownhist.noiltax2 - ownhist.ngastax2  - ;
		                        ownhist.noiltax3 - ownhist.ngastax3  - ;
		                        ownhist.noiltax4 - ownhist.ngastax4  - ;
		                        ownhist.nothtax1 - ownhist.nothtax2  - ;
		                        ownhist.nothtax3 - ownhist.nothtax4  - ;
		                        ownhist.nBackWith - ownhist.nTaxWith -  ;
		                        (ownhist.nExpense + ownhist.nTotale1 + ;
		                          ownhist.nTotale2 + ownhist.nTotale3 + ;
		                          ownhist.nTotale4 + ownhist.nTotale5 + ;
		                          ownhist.nTotaleA + ownhist.nTotaleB + ;
		                          ownhist.ngather  + ownhist.ncompress)
		                CASE ownhist.cdirect = 'B'
		                    lnAmount = ownhist.nothrev + ;
		                        ownhist.ntrprev + ownhist.nmiscrev1 + ;
		                        ownhist.nmiscrev2 + ownhist.nflatrate - ;
		                        ownhist.noiltax1 - ownhist.ngastax1  - ;
		                        ownhist.noiltax2 - ownhist.ngastax2  - ;
		                        ownhist.noiltax3 - ownhist.ngastax3  - ;
		                        ownhist.noiltax4 - ownhist.ngastax4  - ;
		                        ownhist.nothtax1 - ownhist.nothtax2  - ;
		                        ownhist.nothtax3 - ownhist.nothtax4  - ;
		                        ownhist.nBackWith - ownhist.nTaxWith -  ;
		                        (ownhist.nExpense + ownhist.nTotale1 + ;
		                          ownhist.nTotale2 + ownhist.nTotale3 + ;
		                          ownhist.nTotale4 + ownhist.nTotale5 + ;
		                          ownhist.nTotaleA + ownhist.nTotaleB + ;
		                          ownhist.ngather  + ownhist.ncompress)
		
		            ENDCASE
		        CASE ownhist.cRecType = 'N'
		            lnAmount = ownhist.noilrev + ownhist.ngasrev + ;
		                ownhist.ntrprev + ownhist.nmiscrev1 + ;
		                ownhist.nmiscrev2 - ownhist.nflatrate - ;
		                ownhist.nExpense - ownhist.nTotale1 - ;
		                ownhist.nTotale2 - ownhist.nTotale3 - ;
		                ownhist.nTotale4 - ownhist.nTotale5 - ;
		                ownhist.noiltax1 - ownhist.ngastax1  - ;
		                ownhist.noiltax2 - ownhist.ngastax2  - ;
		                ownhist.noiltax3 - ownhist.ngastax3  - ;
		                ownhist.noiltax4 - ownhist.ngastax4  - ;
		                ownhist.nothtax1 - ownhist.nothtax2  - ;
		                ownhist.nothtax3 - ownhist.nothtax4 -  ;
		                ownhist.nBackWith - ownhist.nTaxWith
		        OTHERWISE
		            lnAmount = ownhist.noilrev + ownhist.ngasrev + ownhist.nothrev + ;
		                ownhist.ntrprev + ownhist.nmiscrev1 + ;
		                ownhist.nmiscrev2 + ownhist.nflatrate - ;
		                ownhist.noiltax1 - ownhist.ngastax1  - ;
		                ownhist.noiltax2 - ownhist.ngastax2  - ;
		                ownhist.noiltax3 - ownhist.ngastax3  - ;
		                ownhist.noiltax4 - ownhist.ngastax4  - ;
		                ownhist.nothtax1 - ownhist.nothtax2  - ;
		                ownhist.nothtax3 - ownhist.nothtax4  - ;
		                ownhist.nBackWith - ownhist.nTaxWith -  ;
		                (ownhist.nExpense + ownhist.nTotale1 + ;
		                  ownhist.nTotale2 + ownhist.nTotale3 + ;
		                  ownhist.nTotale4 + ownhist.nTotale5 + ;
		                  ownhist.nTotaleA + ownhist.nTotaleB + ;
		                  ownhist.ngather  + ownhist.ncompress)
		    ENDCASE
		
		    IF ownhist.nIncome <> (ownhist.noilrev + ownhist.ngasrev + ownhist.nothrev + ;
		              ownhist.ntrprev + ownhist.nmiscrev1 + ownhist.nmiscrev2 + ownhist.nflatrate)
		        REPLACE ownhist.nIncome WITH (ownhist.noilrev + ownhist.ngasrev + ownhist.nothrev + ;
		              ownhist.ntrprev + ownhist.nmiscrev1 + ownhist.nmiscrev2 + ownhist.nflatrate)
		    ENDIF
		    IF ownhist.nSevTaxes <> (ownhist.noiltax1 + ownhist.ngastax1 + ;
		              ownhist.noiltax2 + ownhist.ngastax2 +  ownhist.noiltax3 + ownhist.ngastax3  + ;
		              ownhist.noiltax4 + ownhist.ngastax4 + ownhist.nothtax1 + ownhist.nothtax2  + ;
		              ownhist.nothtax3 + ownhist.nothtax4)
		        REPLACE ownhist.nSevTaxes WITH (ownhist.noiltax1 + ownhist.ngastax1 + ;
		              ownhist.noiltax2 + ownhist.ngastax2 +  ownhist.noiltax3 + ownhist.ngastax3  + ;
		              ownhist.noiltax4 + ownhist.ngastax4 + ownhist.nothtax1 + ownhist.nothtax2  + ;
		              ownhist.nothtax3 + ownhist.nothtax4)
		    ENDIF
		
		    IF ownhist.nnetcheck <> lnAmount
		        THISFORM.cmdatamanager.REPLACE('ownhist', 'nNetcheck', lnAmount)
		    ENDIF
		    THISFORM.txtnNetCheck.REFRESH()
		
		CATCH TO loError
		    llReturn = .F.
		    DO errorlog WITH 'Caltot', loError.LINENO, 'Owner History', loError.ERRORNO, loError.MESSAGE, '', loError
		    MESSAGEBOX('Unable to process the history at this time. Check the System Log found under Other Reports for more information.' + CHR(10) + CHR(10) + ;
		          'Contact SherWare Support for help at support@sherware.com', 16, 'Problem Encountered')
		ENDTRY
		
		RETURN llReturn
	ENDPROC

	PROCEDURE findinterests		&& Fills in interests and owner type from wellinv.
		LOCAL llReturn, loError
		llReturn = .T.
		
		TRY
		    SELECT wellinv
		    LOCATE FOR cOwnerID == ownhist.cOwnerID AND cWellID == ownhist.cWellID AND cTypeInv = ownhist.cTypeInv
		    IF FOUND()
		        IF nRevOil <> THISFORM.pgfpageframecustom1.page3.txtrevoil2.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtrevoil2.SET('Value', wellinv.nRevOil)
		        ENDIF
		        IF nRevGas <> THISFORM.pgfpageframecustom1.page3.txtrevGas.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtrevGas.SET('Value', wellinv.nRevGas)
		        ENDIF
		        IF nRevOth <> THISFORM.pgfpageframecustom1.page3.txtrevoth.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtrevoth.SET('Value', wellinv.nRevOth)
		        ENDIF
		        IF nRevTrp <> THISFORM.pgfpageframecustom1.page3.txtrevTrp.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtrevTrp.SET('Value', wellinv.nRevTrp)
		        ENDIF
		        IF nRevMisc1 <> THISFORM.pgfpageframecustom1.page3.txtRevMisc1.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtRevMisc1.SET('Value', wellinv.nRevMisc1)
		        ENDIF
		        IF nRevMisc2 <> THISFORM.pgfpageframecustom1.page3.txtRevMisc2.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtRevMisc2.SET('Value', wellinv.nRevMisc2)
		        ENDIF
		        IF nWorkInt <> THISFORM.pgfpageframecustom1.page3.txtWorkInt.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtWorkInt.SET('Value', wellinv.nWorkInt)
		        ENDIF
		        IF nIntClass1 <> THISFORM.pgfpageframecustom1.page3.txtIntClass1.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtIntClass1.SET('Value', wellinv.nIntClass1)
		        ENDIF
		        IF nIntClass2 <> THISFORM.pgfpageframecustom1.page3.txtIntClass2.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtIntClass2.SET('Value', wellinv.nIntClass2)
		        ENDIF
		        IF nIntClass3 <> THISFORM.pgfpageframecustom1.page3.txtIntClass3.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtIntClass3.SET('Value', wellinv.nIntClass3)
		        ENDIF
		        IF nIntClass4 <> THISFORM.pgfpageframecustom1.page3.txtIntClass4.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtIntClass4.SET('Value', wellinv.nIntClass4)
		        ENDIF
		        IF nIntClass5 <> THISFORM.pgfpageframecustom1.page3.txtIntClass5.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtIntClass5.SET('Value', wellinv.nIntClass5)
		        ENDIF
		        IF nBCPInt <> THISFORM.pgfpageframecustom1.page3.txtnBCP.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtnBCP.SET('Value', wellinv.nBCPInt)
		        ENDIF
		        IF nACPInt <> THISFORM.pgfpageframecustom1.page3.txtnACP.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtnACP.SET('Value', wellinv.nACPInt)
		        ENDIF
		        IF nAPOInt <> THISFORM.pgfpageframecustom1.page3.txtnAPO.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtnAPO.SET('Value', wellinv.nAPOInt)
		        ENDIF
		        IF nRevTax1 <> THISFORM.pgfpageframecustom1.page3.txtnOilTax1.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtnOilTax1.SET('Value', wellinv.nRevTax1)
		        ENDIF
		        IF nRevTax4 <> THISFORM.pgfpageframecustom1.page3.txtnOilTax2.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtnOilTax2.SET('Value', wellinv.nRevTax4)
		        ENDIF
		        IF nRevTax7 <> THISFORM.pgfpageframecustom1.page3.txtnOilTax3.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtnOilTax3.SET('Value', wellinv.nRevTax7)
		        ENDIF
		        IF nRevTax10 <> THISFORM.pgfpageframecustom1.page3.txtnOilTax4.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtnOilTax4.SET('Value', wellinv.nRevTax10)
		        ENDIF
		        IF nRevTax2 <> THISFORM.pgfpageframecustom1.page3.txtnGasTax1.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtnGasTax1.SET('Value', wellinv.nRevTax2)
		        ENDIF
		        IF nRevTax5 <> THISFORM.pgfpageframecustom1.page3.txtnGasTax2.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtnGasTax2.SET('Value', wellinv.nRevTax5)
		        ENDIF
		        IF nRevTax8 <> THISFORM.pgfpageframecustom1.page3.txtnGasTax3.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtnGasTax3.SET('Value', wellinv.nRevTax8)
		        ENDIF
		        IF nRevTax11 <> THISFORM.pgfpageframecustom1.page3.txtnGasTax4.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtnGasTax4.SET('Value', wellinv.nRevTax11)
		        ENDIF
		        IF nRevTax3 <> THISFORM.pgfpageframecustom1.page3.txtnOthTax1.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtnOthTax1.SET('Value', wellinv.nRevTax3)
		        ENDIF
		        IF nRevTax6 <> THISFORM.pgfpageframecustom1.page3.txtnOthTax2.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtnOthTax2.SET('Value', wellinv.nRevTax6)
		        ENDIF
		        IF nRevTax9 <> THISFORM.pgfpageframecustom1.page3.txtnOthTax3.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtnOthTax3.SET('Value', wellinv.nRevTax9)
		        ENDIF
		        IF nRevTax12 <> THISFORM.pgfpageframecustom1.page3.txtnOthTax4.VALUE
		            THISFORM.pgfpageframecustom1.page3.txtnOthTax4.SET('Value', wellinv.nRevTax12)
		        ENDIF
		        IF cTypeInv <> THISFORM.cboownType.VALUE
		            THISFORM.cboownType.SET('Value', cTypeInv)
		        ENDIF
		        IF cTypeInt <> THISFORM.cboIntType.VALUE
		            THISFORM.cboIntType.SET('Value', cTypeInt)
		        ENDIF
		        IF cDirect <> THISFORM.cboDirect.VALUE
		            THISFORM.cboDirect.SET('Value', cDirect)
		        ENDIF
		    ENDIF
		CATCH TO loError
		    llReturn = .F.
		    DO errorlog WITH 'FindInterests', loError.LINENO, 'Owner History', loError.ERRORNO, loError.MESSAGE, '', loError
		    MESSAGEBOX('Unable to find the owners interests at this time. Check the System Log found under Other Reports for more information.' + CHR(10) + CHR(10) + ;
		          'Contact SherWare Support for help at support@sherware.com', 16, 'Problem Encountered')
		ENDTRY
		
		RETURN llReturn
	ENDPROC

