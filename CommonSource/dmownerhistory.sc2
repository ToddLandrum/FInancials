	PROCEDURE CMDATAMANAGER.afternav
		LPARAMETERS cworkarea
		
		LOCAL lnx
		IF m.cworkarea = 'Ownhist'
		    THISFORM.txtcWellID.REFRESH()
		    THISFORM.txtcOwnerID.REFRESH()
		
		    IF NOT EMPTY(THISFORM.txtcWellID.VALUE)
		        SELECT wells
		        SET ORDER TO cWellID
		        IF SEEK(THISFORM.txtcWellID.VALUE)
		            THISFORM.txtcWellName.VALUE = wells.cWellName
		            THISFORM.txtcWellName.REFRESH()
		        ELSE
		            THISFORM.txtcWellName.VALUE = ''
		            THISFORM.txtcWellName.REFRESH()
		        ENDIF
		    ENDIF
		
		    IF NOT EMPTY(THISFORM.txtcOwnerID.VALUE)
		        SELECT investor
		        SET ORDER TO cOwnerID
		        IF SEEK(THISFORM.txtcOwnerID.VALUE)
		            THISFORM.txtcOwnerName.VALUE = investor.cOwnname
		            THISFORM.txtcOwnerName.REFRESH()
		        ELSE
		            THISFORM.txtcOwnerName.VALUE = ''
		            THISFORM.txtcOwnerName.REFRESH()
		        ENDIF
		    ENDIF
		
		    THISFORM.cboRecType.REFRESH()
		
		    DO CASE
		        CASE THISFORM.cboRecType.LISTITEM[thisform.cboRecType.listitemid] = 'N'
		            THISFORM.lblNetCheck.CAPTION = 'Net Amount'
		            THISFORM.lblNetCheck.REFRESH()
		        CASE THISFORM.cboRecType.LISTITEM[thisform.cboRecType.listitemid] = 'J'
		            THISFORM.lblNetCheck.CAPTION = 'JIB Amount'
		            THISFORM.lblNetCheck.REFRESH()
		        CASE THISFORM.cboRecType.LISTITEM[thisform.cboRecType.listitemid] = 'R'
		            THISFORM.lblNetCheck.CAPTION = 'Rev Amount'
		            THISFORM.lblNetCheck.REFRESH()
		    ENDCASE
		   
		    thisform.runcombo1.Requery()
		    FOR lnx = 1 TO THISFORM.runcombo1.LISTCOUNT
		        IF THISFORM.runcombo1.LISTITEM[lnx, 2] = ownhist.cRunYear AND ;
		                INT(VAL(THISFORM.runcombo1.LISTITEM[lnx, 3])) = ownhist.nRunNo AND ;
		                THISFORM.runcombo1.LISTITEM[lnx, 4] = ownhist.cgroup AND ;
		                thisform.runcombo1.ListItem[lnx, 6] = ownhist.crectype
		            THISFORM.runcombo1.LISTITEMID = lnx
		            EXIT
		        ENDIF
		    ENDFOR
		ENDIF
	ENDPROC

	PROCEDURE CMDATAMANAGER.afternew
		LPARAMETERS cworkarea
		LOCAL lciddisb
		
		IF UPPER(m.cworkarea) = 'OWNHIST'
		   lciddisb = THISFORM.oRegistry.IncrementCounter('%Shared.Counters.Owner History')
		
		   SELECT disbhist1
		   SET ORDER TO ciddisb
		   SET DELE OFF
		   DO WHILE SEEK(lciddisb)
		      lciddisb = THISFORM.oRegistry.IncrementCounter('%Shared.Counters.Owner History')
		   ENDDO
		   SET DELE ON
		
		   SELECT ownhist
		   REPLACE ciddisb_a WITH lciddisb, ciddisb_b WITH lciddisb, lManual WITH .t.
		   THISFORM.txtcOwnerName.VALUE = ''
		   THISFORM.txtcWellName.VALUE  = ''
		   thisform.cborecType.set('value','R')
		   thisform.cboownType.set('value','W')
		   thisform.cboIntType.set('value','B')
		   thisform.cbodirect.set('value','N')
		   thisform.txthyear.set('value',ALLTRIM(STR(year(DATE()))))
		   thisform.txtHperiod.set('value',PADL(ALLTRIM(STR(month(DATE()))),2,'0'))
		   THISFORM.setnextcontrol(THISFORM.txtcOwnerID)     
		ENDIF
		
	ENDPROC

	PROCEDURE CMDATAMANAGER.beforedelete
		LPARAMETERS cworkarea
		
		IF NOT THISFORM.oMessage.CONFIRM('Deleting owner history records can affect various reports, including 1099s.  Are you sure you want to delete this record?')
		   RETURN .F.
		ENDIF
		
		IF NOT EMPTY(ownhist.cSuspType)  &&  It came from suspense, and was probably created by clearing a suspense balance, so put it back into suspense when it gets deleted
		   SELECT ownhist
		   SCATTER memvar
		   
		   m.ciddisb = THISFORM.oRegistry.IncrementCounter('%Shared.Counters.Owner History')  &&  Make sure new primary key in suspense is unique
		   SELECT suspense
		   SET ORDER TO ciddisb
		   SET DELE OFF
		   DO WHILE SEEK(m.ciddisb)
		      m.ciddisb= THISFORM.oRegistry.IncrementCounter('%Shared.Counters.Owner History')
		   ENDDO
		   SET DELE ON
		   
		   INSERT INTO suspense FROM memvar
		endif
		
		DODEFAULT(cworkarea)
		
	ENDPROC

	PROCEDURE CMDATAMANAGER.beforeupdate
		LPARAMETERS cWorkareaList, lCurrentRecordOnly
		
		IF ownhist.nNetCheck = 0 AND ownhist.noilrev = 0 AND ownhist.ngasrev = 0 
		   thisform.oMessage.Warning('You cannot save a zero amount entry.  Please enter revenue, expenses, or taxes into this record.')
		   RETURN .f.
		ENDIF
		
		tcYear          = THISFORM.runcombo1.LISTITEM[thisform.runcombo1.listitemid, 2]
		tnRunNo         = INT(VAL(THISFORM.runcombo1.LISTITEM[thisform.runcombo1.listitemid, 3]))
		tcGroup         = THISFORM.runcombo1.LISTITEM[thisform.runcombo1.listitemid, 4]
		tcDate          = THISFORM.runcombo1.LISTITEM[thisform.runcombo1.listitemid, 7]
		
		SELECT ownhist   
		replace crunyear WITH tcYear
		replace nrunno   WITH tnRunNo
		replace cGroup   WITH tcGroup
		replace hdate    WITH CTOD(tcDate)
		
		DODEFAULT(cworkarealist, lcurrentrecordonly)
	ENDPROC

