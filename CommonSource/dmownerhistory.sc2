	PROCEDURE txtcWellID.Valid
		LOCAL lnx
		LOCAL cRectype, hDate, tcGroup, tcYear, tnRunNo
		
		IF THISFORM.isnew()  &&  If adding a new record, fill in the interests automatically
		    SELECT wellinv
		    LOCATE FOR cwellid = THIS.VALUE AND cownerid = THISFORM.txtcownerid.VALUE
		    IF FOUND()
		        SCATTER MEMVAR
		        IF m.ljib
		            m.cRectype = 'J'
		        ELSE
		            m.cRectype = 'R'
		        ENDIF
		
		        m.hDate = DATE()
		        SELECT ownhist
		        GATHER MEMVAR
		        THISFORM.Findinterests()
		        THISFORM.REFRESH()
		    ENDIF
		ENDIF
		
		SELECT wells
		LOCATE FOR cwellid = THIS.VALUE
		IF ownhist.cGroup <> wells.cGroup
		    SELECT ownhist
		    REPLACE cGroup WITH wells.cGroup
		    THISFORM.cGroup = wells.cGroup
		
		    FOR lnx = 1 TO THISFORM.runcombo1.LISTCOUNT
		        IF THISFORM.runcombo1.LISTITEM[lnx, 4] = wells.cGroup AND ;
		                THISFORM.runcombo1.LISTITEM[lnx, 6] = 'R'
		            THISFORM.runcombo1.LISTITEMID = lnx
		            tcYear                        = THISFORM.runcombo1.LISTITEM[lnx, 2]
		            tnRunNo                       = INT(VAL(THISFORM.runcombo1.LISTITEM[lnx, 3]))
		            tcGroup                       = THISFORM.runcombo1.LISTITEM[lnx, 4]
		            tcType                        = THISFORM.runcombo1.LISTITEM[lnx, 6]
		            SELECT ownhist
		            REPLACE crunyear WITH tcYear
		            REPLACE nrunno   WITH tnRunNo
		            REPLACE cGroup   WITH tcGroup
		            replace cRecType WITH tcType
		            EXIT
		        ENDIF
		    ENDFOR
		ENDIF
		
		DODEFAULT()
		
		
		 
	ENDPROC

