	PROCEDURE Activate
		ON KEY LABEL F5 _screen.ActiveForm.F5()
		
		DODEFAULT()
	ENDPROC

	PROCEDURE f5		&& Enables the qty on hand field for editing, with a warning.
		thisform.omessage.Warning('Manually editing the quantity on hand can cause differences between purchased inventory amounts and quantity on hand amounts.')
		
		ThisForm.pgfPageFrame.Page1.txtNqtyonhand.enabled = .t.
		thisform.pgfPageFrame.Page1.txtNqtyonhand.setfocus()
	ENDPROC

	PROCEDURE list
		LPARAMETERS cWorkarea
		*
		PRIV llOK
		LOCAL lcList, lnRecNo, lStatus
		* Default to the form's primary workarea
		IF EMPTY(m.cWorkarea)
		   m.cWorkarea = THIS.cWorkarea
		ENDIF
		
		IF THIS.FlushControlBuffer()
		   * If this or any child is changed, Validate it and children
		   IF THIS.BeforeNav(m.cWorkarea)
		      THIS.RefreshAll()  && In case BeforeNav() did a Revert()
		
		      llOK   = .T.
		      lcList = 'citemid,citemdesc'
		
		      SELECT items
		      lnRecNo = RECNO()
		      COUNT FOR NOT DELETED() TO lnCount
		      IF lnCount = 0
		         RETURN
		      ENDIF
		
		      DO FORM ..\CUSTOM\picklist WITH 'Items', lcList, THISFORM.pgfpageframe.page1.txtcitemid.VALUE, 2
		
		      IF NOT llOK
		         SELECT items
		         GOTO lnRecNo
		      ENDIF
		
		      THISFORM.RefreshAll()
		   ENDIF
		ENDIF
		
	ENDPROC

	PROCEDURE loaddisposal
		SELECT cdispsite, cdisperm FROM disposal INTO CURSOR temp
		lnX = 1
		thisform.pgfpageframe.page1.cbodisposal.addlistitem('      ',lnx,1)
		thisform.pgfpageframe.page1.cbodisposal.addlistitem('      ',lnx,2)
		lnx = 2
		SELECT temp
		SCAN
		   thisform.pgfpageframe.page1.cbodisposal.addlistitem(ALLTRIM(cdispsite)+' - '+cdisperm,lnX,1)
		   thisform.pgfpageframe.page1.cbodisposal.addlistitem(cdisperm,lnX,2)
		   lnX = lnX + 1
		ENDSCAN
		
		thisform.pgfpageframe.page1.cbodisposal.refresh()
		   
	ENDPROC

	PROCEDURE lookcoa
		LPARA tcField
		LOCAL lcList
		PRIV llOK
		
		llOK   = .T.
		lcList = 'cacctno/acctno,cacctdesc/acctdesc'
		
		
		IF UPPER(tcField) = 'SALES'
		   DO FORM ..\CUSTOM\picklist WITH 'Coa', lcList, THISFORM.pgfpageframe.page1.txtcdefacct.VALUE, 2
		   IF llOK
		      THISFORM.pgfpageframe.page1.txtcdefacct.VALUE = coa.cacctno
		      THISFORM.pgfpageframe.page1.txtcDefAcctDesc.VALUE = coa.cAcctDesc
		      THISFORM.cmdatamanager.REPLACE('items', 'cDefAcct', coa.cacctno)
		      THISFORM.pgfpageframe.page1.txtcdefacct.REFRESH()
		      THISFORM.pgfpageframe.page1.txtcDefAcctDesc.REFRESH()
		      THISFORM.pgfpageframe.page1.txtcdefacct.SETFOCUS()
		   ENDIF
		ELSE
		   DO FORM ..\CUSTOM\picklist WITH 'Coa', lcList, THISFORM.pgfpageframe.page1.txtccogsacct.VALUE, 2
		   IF llOK
		      THISFORM.pgfpageframe.page1.txtccogsacct.VALUE = coa.cacctno
		      THISFORM.pgfpageframe.page1.txtcCOGSDesc.VALUE = coa.cAcctDesc
		      THISFORM.cmdatamanager.REPLACE('items', 'cCOGSAcct', coa.cacctno)
		      THISFORM.pgfpageframe.page1.txtccogsacct.REFRESH()
		      THISFORM.pgfpageframe.page1.txtcCOGSDesc.REFRESH()
		      THISFORM.pgfpageframe.page1.txtccogsacct.SETFOCUS()
		   ENDIF
		ENDIF
		
		
		
	ENDPROC

	PROCEDURE showdesc
		LOCAL lcSelect
		
		lcSelect = SELECT()
		TRY 
		WITH THISFORM.pgfpageframe.page1
		
		   .txtcDefAcct.REFRESH()  &&  Default sales account
		   swselect('coa')
		   SET ORDER TO acctno
		   IF SEEK(items.cdefacct)
		      .txtcDefAcctDesc.VALUE = coa.cAcctDesc
		      .txtcDefAcctDesc.REFRESH()
		   ELSE
		      .txtcDefAcctDesc.VALUE = ''
		      .txtcDefAcctDesc.REFRESH()
		   ENDIF
		
		   .txtcCOGSAcct.REFRESH()  &&  COGS Account
		   IF SEEK(items.ccogsacct)
		      .txtcCOGSDesc.VALUE = coa.cAcctDesc
		      .txtcCOGSDesc.REFRESH()
		   ELSE
		      .txtcCOGSDesc.VALUE = ''
		      .txtcCOGSDesc.REFRESH()
		   ENDIF
		
		   swselect('vendor')  &&  Vendor Name
		   IF SEEK(items.cvendorid)
		      .txtcVendName.VALUE = vendor.cVendName
		      .txtcVendName.REFRESH()
		   ELSE
		      .txtcVendName.VALUE = ''
		      .txtcVendName.REFRESH()
		   ENDIF
		
		   IF items.ctype = 'B'
		      .chkInjFee.Visible = .t.
		   ELSE
		      .chkInjFee.Visible = .f.
		   ENDIF 
		ENDWITH
		
		SELECT (lcSelect)
		CATCH
		ENDTRY 
	ENDPROC

