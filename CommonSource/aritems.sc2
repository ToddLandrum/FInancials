	PROCEDURE pgfPageFrame.Page1.txtCitemid.Valid
		lcItem = THIS.VALUE
		IF NOT EMPTY(lcItem) AND lcItem <> items.citemid
		   SET DELETED OFF
		   SELECT items1
		   SET ORDER TO citemid
		   IF SEEK(ALLTRIM(lcItem))
		      IF NOT DELETED()
		         IF THISFORM.omessage.CONFIRM('There is already an item with this ID.  Would you like to edit the existing record?')
		            THISFORM.cmdatamanager.revertall()
		            SELECT items
		            SET ORDER TO citemid
		            LOCATE FOR citemid= lcItem
		            THISFORM.REFRESH()
		         ELSE
		            THIS.VALUE = ''
		            RETURN 0
		         ENDIF
		      ELSE
		         thisform.cmdatamanager.revertall()
		         select items
		         locate for citemid = lcItem
		         SCATTER MEMVAR BLANK
		         m.cType = 'P'
		         m.cTaxable = 'T'
		         GATHER MEMVAR
		         RECALL
		      ENDIF
		   ENDIF
		   SET DELETED ON
		   SELECT items
		ENDIF
		
		DODEFAULT()
		
		
	ENDPROC

