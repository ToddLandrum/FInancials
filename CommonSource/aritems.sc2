	PROCEDURE pgfPageFrame.Page1.cmdLookVend.Click
		LOCAL lcList, lcSelect
		PRIV llOK
		
		llOK = .T.
		
		lcSelect = SELECT()
		lcList = 'cvendorid,cvendname'
		
		DO FORM ..\custom\picklist WITH 'Vendor', lcList, thisform.pgfpageframe.page1.txtcvendorid.value, 2
		
		IF llOK
		   IF Vendor.cvendorid <> items.cvendorid
		      THISFORM.pgfpageframe.page1.txtcVendorId.VALUE = vendor.cvendorid
		      THISFORM.cmdatamanager.REPLACE('items', 'cvendorid', vendor.cvendorid)
		      THISFORM.pgfpageframe.page1.txtcVendName.VALUE = vendor.cVendName
		      THISFORM.pgfpageframe.page1.txtcVendorId.REFRESH()
		      THISFORM.pgfpageframe.page1.txtcVendName.REFRESH()
		      THISFORM.lchanged = .T.
		   ENDIF
		ELSE
		   THISFORM.pgfpageframe.page1.txtcVendorId.VALUE = ''
		   THISFORM.pgfpageframe.page1.txtcVendName.VALUE = ''
		   THISFORM.pgfpageframe.page1.txtcVendorId.REFRESH()
		   THISFORM.pgfpageframe.page1.txtcVendName.REFRESH()
		ENDIF
		
		SELECT (lcSelect)
		
		
	ENDPROC

