	PROCEDURE cmdatamanager.beforeupdate
		LPARAMETERS cworkarealist
		
		RETURN .t.
		IF NOT EMPTY(custs.cCustID)
		   IF THISFORM.cmdatamanager.isnew('Custs')  &&  Only check this on new records, so it doesn't complain on updates to existing ones
		      SELECT investor
		      LOCATE FOR cOwnerID == custs.cCustID
		      IF FOUND()  &&  Don't let them have duplicate IDs
		         THISFORM.oMessage.Warning('There is already an owner defined with this same ID.  To avoid reporting errors, you cannot use the same ID for a customer.')
		         THISFORM.setnextcontrol(THISFORM.PAGEFRAME.Page1.txtCcustid)
		         RETURN .F.
		      ENDIF
		
		      SELECT vendor
		      LOCATE FOR cVendorID == custs.cCustID
		      IF FOUND()  &&  Don't let them have duplicate IDs
		         THISFORM.oMessage.Warning('There is already a vendor defined with this same ID.  To avoid reporting errors, you cannot use the same ID for a customer.')
		         THISFORM.setnextcontrol(THISFORM.PAGEFRAME.Page1.txtCcustid)
		         RETURN .F.
		      ENDIF
		   ENDIF
		ENDIF
		
		IF TRANSFORM(THISFORM.PAGEFRAME.page3.spinner1.VALUE) <> custs.cPriceDef
		   IF NOT INLIST(TRANSFORM(THISFORM.PAGEFRAME.page3.spinner1.VALUE),'1','2','3','4','5')
		      THISFORM.PAGEFRAME.page3.spinner1.VALUE = 1
		   endif
		   THISFORM.cmdatamanager.REPLACE('Custs','Cpricedef',TRANSFORM(THISFORM.PAGEFRAME.page3.spinner1.VALUE))
		ENDIF
		
		IF EMPTY(custs.caddress1) AND NOT EMPTY(cbAddr1)
		   IF THISFORM.oMessage.CONFIRM('Should the billing address be copied to the shipping address?')
		      THISFORM.cmdatamanager.REPLACE('custs','Caddress1',custs.cbAddr1)
		      THISFORM.cmdatamanager.REPLACE('custs','Caddress2',custs.cbaddr2)
		      THISFORM.cmdatamanager.REPLACE('custs','Ccity',custs.cbcity)
		      THISFORM.cmdatamanager.REPLACE('custs','Cstate',custs.cbstate)
		      THISFORM.cmdatamanager.REPLACE('custs','Czip',custs.cbzip)
		   ENDIF
		ENDIF
		
		SELECT custs1
		LOCATE FOR ALLTRIM(UPPER(custs.cCustName)) $ UPPER(cCustName) AND RECNO('custs1') <> RECNO('custs')  &&  Check for duplicate name, and give them a warning about possibly screwing up reports
		IF FOUND()
		   THISFORM.oMessage.Warning('The name you have assigned this customer already exists as part of another customer name.  Be aware that duplicate names can cause reporting errors when reports are sorted by name.')
		ENDIF
		
		RETURN .T.
		
	ENDPROC

	PROCEDURE cmdatamanager.fieldvalid
		LPARAMETERS cworkarea,cfield,uvalue
		
		IF UPPER(m.cworkarea) == 'CUSTS'
		    DO CASE
		
		        CASE UPPER(m.cfield)=UPPER('cpricedef')
		            IF NOT INLIST(m.uvalue,'1','2','3','4','5')
		               thisform.oMessage.Display('The Price Default Must Be Between 1 and 5')
		               RETURN 0
		            ENDIF
		    ENDCASE
		ENDIF
		
	ENDPROC

