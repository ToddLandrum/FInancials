	PROCEDURE pageFrame.Page1.txtCcustid.Valid
		lcCustid = THIS.VALUE
		
		IF NOT EMPTY(lcCustid) AND lcCustid <> custs.ccustid
		   SELECT custs1
		   SET ORDER TO ccustid
		   SET DELETED OFF
		   IF SEEK(lcCustid)
		      IF NOT DELETED()
		         IF THISFORM.omessage.CONFIRM('There is already a customer with this ID.  Would you like to edit the existing record?')
		            THISFORM.cmdatamanager.revertall()
		            SELECT custs
		            SET ORDER TO ccustid
		            SEEK(lcCustid)
		            THISFORM.REFRESH()
		         ELSE
		            THIS.VALUE = ''
		            RETURN 0
		         ENDIF
		      ELSE
		         SELECT custs
		         SCATTER MEMVAR
		         THISFORM.cmdatamanager.revertall()
		         m.ccustid = lcCustid
		         LOCATE FOR ccustid = lcCustid
		         RECALL
		         GATHER MEMVAR
		         THISFORM.REFRESH()
		      ENDIF
		   ENDIF
		   SELECT custs
		   SET DELETED ON
		ENDIF
		
		DODEFAULT()
		
	ENDPROC

