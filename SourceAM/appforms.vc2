	lblApplication.Name = "lblApplication"
	lblVersion.Caption = "Version 5.4"
	lblVersion.Left = 160
	lblVersion.Name = "lblVersion"
	
	PROCEDURE lblVersion.Init
		LOCAL lcVersion
		
		lnCount = OCCURS('.',m.goApp.cFileVersion)
		
		DO CASE
		   CASE lnCount = 3
		      lcVersion = SUBSTR(m.goApp.cFileVersion,1,RAT('.',m.goApp.cFileVersion,2)-1)
		   CASE lnCount = 2   
		      lcVersion = SUBSTR(m.goApp.cFileVersion,1,RAT('.',m.goApp.cFileVersion)-1)
		   OTHERWISE
		      lcVersion = m.goApp.cFileVersion   
		ENDCASE
		
		THIS.Caption = 'Version ' + lcVersion
		DODEFAULT()
	ENDPROC

ENDDEFINE

