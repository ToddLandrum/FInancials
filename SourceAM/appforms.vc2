DEFINE CLASS frmrptcriteria AS swrptcriteria OF "..\custom\swforms.vcx" 
 	*< CLASSDATA: Baseclass="form" Timestamp="" Scale="Pixels" Uniqueid="" />

	*<DefinedPropArrayMethod>
		*m: callhelp
		*p: calias
		*p: lquiet
		*p: lsummary
	*</DefinedPropArrayMethod>

	calias = 
	Caption = "Frmform1"
	cfriendlyname = 
	DataSession = 2
	DoCreate = .T.
	Icon = ..\graphics\am.ico
	lquiet = .F.
	lsummary = .F.
	Name = "frmrptcriteria"
	_memberdata = <VFPData>
		<memberdata name="cfriendlyname" display="cFriendlyName"/>
		</VFPData>
	Swrptcriteriabuttons1.chkExport.Alignment = 0
	Swrptcriteriabuttons1.chkExport.Name = "chkExport"
	Swrptcriteriabuttons1.cmdclose.Name = "cmdclose"
	Swrptcriteriabuttons1.cmdPreview.Name = "cmdPreview"
	Swrptcriteriabuttons1.cmdPrint.Name = "cmdPrint"
	Swrptcriteriabuttons1.Name = "Swrptcriteriabuttons1"
	
	PROCEDURE callhelp
		LPARAMETERS tcName
		
		IF FILE('helptopic.txt')
		    WAIT WINDOW 'Showing Help Topic: ' + TRANSFORM(THIS.HELPCONTEXTID)
		ENDIF
		IF NOT EMPTY(THIS.HELPCONTEXTID)
		    HELP ID THISFORM.HELPCONTEXTID
		ELSE
		    HELP
		ENDIF
		
	ENDPROC

	PROCEDURE Error
		LPARAMETERS nerror,cmethod,nline
		
		=aerror(gaerrors)
		
		DO errorlog WITH m.cMethod, m.nLine, this.name
		
		DODEFAULT(nError,cMethod,nLine)      
	ENDPROC

	PROCEDURE Init
		IF DODEFAULT()
		   SET DELETED ON
		   this.omessage = FINDGLOBALOBJECT('cmMessage')
		   this.oregistry = FINDGLOBALOBJECT('cmRegistry')
		   thisform.swrptcriteriabuttons1.left = (this.width/2 - thisform.swrptcriteriabuttons1.width/2)
		   this.height = thisform.swrptcriteriabuttons1.top + thisform.swrptcriteriabuttons1.height + 10
		   ON KEY LABEL F1 _screen.ActiveForm.callhelp()
		ENDIF
	ENDPROC

	PROCEDURE isenabled
		LPARAMETERS cmethod,cworkarea
		*	The 'isenabled' method determines whether the specified form method can be
		*	called from a button or menu.
		*	When the forms 'Report' method is called, the forms 'isenabled' method must be
		*	set to .T..
		
		IF UPPER(m.cmethod) = "REPORT"
			RETURN .T.
		ELSE
			RETURN DODEFAULT(cmethod,cworkarea)
		ENDIF
		
	ENDPROC

	PROCEDURE Load
		IF DODEFAULT()
		   SET MULTILOCKS ON
		ENDIF   
	ENDPROC

	PROCEDURE Unload
		ON KEY LABEL F1 
		ON KEY LABEL F2
		ON KEY LABEL ENTER
		ON KEY LABEL SPACEBAR
		DODEFAULT()
	ENDPROC

ENDDEFINE
