	ENDPROC

	PROCEDURE pgfAssets.pagGeneral.txtCassetno.Valid
		lcAssetno = THIS.VALUE
		
		IF NOT EMPTY(lcAssetno) AND lcAssetno <> assets.cunitno
		   SELECT assets1
		   SET ORDER TO unitno
		   SET DELETED OFF
		   IF SEEK(THIS.VALUE)
		      IF NOT DELETED()
		         IF THISFORM.omessage.CONFIRM('There is already a fixed asset with this ID.  Would you like to view the existing record?')
		            THISFORM.cmdatamanager.revertall()
		            SELECT assets
		            SET ORDER TO unitno
		            SEEK(lcAssetno)
		            THISFORM.REFRESH()
		         ELSE
		            THIS.VALUE = ''
		            RETURN 0
		         ENDIF
		      ELSE
		         THISFORM.cmdatamanager.revertall()
		         SELECT assets
		         SCATTER MEMVAR
		         m.cunitno = lcAssetno
		         GATHER MEMVAR
		      ENDIF
		   ENDIF
		   SELECT assets
		ENDIF
		
		DODEFAULT()
		
	ENDPROC

