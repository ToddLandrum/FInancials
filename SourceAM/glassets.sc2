	PROCEDURE list
		LPARAMETERS cWorkarea
		*
		PRIV llOK
		LOCAL lcList, lnRecNo, lStatus
		* Default to the form's primary workarea
		IF EMPTY(m.cWorkarea)
		   m.cWorkarea = THIS.cWorkarea
		ENDIF
		
		IF THIS.FlushControlBuffer()
		   * If this or any child is changed, Validate it and children
		   IF THIS.BeforeNav(m.cWorkarea)
		      THIS.RefreshAll()  && In case BeforeNav() did a Revert()
		
		      llOK = .F.
		      lcList = 'cunitno/unitno,cunitdesc'
		
		      SELECT assets
		      lnRecNo = RECNO()
		      COUNT FOR NOT DELETED() TO lnCount
		      IF lnCount = 0
		         RETURN
		      ENDIF
		
		      DO FORM ..\CUSTOM\picklist WITH 'Assets', lcList, 'cunitno', 2
		
		      IF NOT llOK
		         SELECT assets
		         GOTO lnRecNo
		      ENDIF
		
		      THISFORM.RefreshAll()
		   ENDIF
		ENDIF
		
	ENDPROC

	PROCEDURE cmdatamanager.afternew
		LPARAMETERS cworkarea
		
		THISFORM.pgfAssets.pagGeneral.cboNewUsed.set('value','N')
		THISFORM.pgfAssets.pagGeneral.cboType.LISTITEMID = 1
		thisform.setnextcontrol(thisform.pgfAssets.paggeneral.txtcAssetNo)
		DODEFAULT()
		
	ENDPROC

	PROCEDURE pgfAssets.pagGeneral.cboNewUsed.Init
		THIS.ADDLISTITEM('New',1,1)
		THIS.ADDLISTITEM('N',1,2)
		THIS.ADDLISTITEM('Used',2,1)
		THIS.ADDLISTITEM('U',2,2)
		
		DODEFAULT()
	ENDPROC

